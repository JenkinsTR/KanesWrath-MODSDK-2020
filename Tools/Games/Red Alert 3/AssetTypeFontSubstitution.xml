<?xml version="1.0" encoding="utf-8" ?>
<AssetDefinition xmlns="uri:thundermods.net:SAGE:WrathEdXML:AssetDefinition">

	<Asset
		id="Subtitution">
		<Entry
			id="Size"
			AssetType="SageUnsignedInt"
			IsAttribute="true"
			IsRequired="true" />
		<Entry
			id="Font"
			AssetType="FontDesc" />
	</Asset>

	<Asset
		id="FontSubstitution">
		<EntryInheritance
			id="base"
			AssetType="BaseInheritableAsset" />
		<Entry
			id="Name"
			AssetType="String"
			IsAttribute="true"
			IsRequired="true" />
		<EntryRelocation
			id="Substitute"
			AssetType="Subtitution" />
	</Asset>

</AssetDefinition>