<?xml version="1.0" encoding="utf-8" ?>
<AssetDefinition xmlns="uri:thundermods.net:SAGE:WrathEdXML:AssetDefinition">

	<EnumAsset
		id="ShroudStatusType">
		<Entry>FOGGED</Entry>
		<Entry>CLEAR</Entry>
		<Entry>SHROUDED</Entry>
	</EnumAsset>

	<Asset
		id="W3DDeviceSettings">
		<EntryInheritance
			id="base"
			AssetType="BaseSingletonSettings" />
		<Entry
			id="MaxTerrainTracks"
			AssetType="SageInt"
			IsAttribute="true"
			Default="100" />
		<Entry
			id="OpacityOfSimpleMergeDecals"
			AssetType="Percentage"
			IsAttribute="true"
			Default="35%" />
		<Entry
			id="OccludedColorLuminanceScale"
			AssetType="SageReal"
			IsAttribute="true"
			Default="0.5" />
		<Entry
			id="MaxRoadSegments"
			AssetType="SageInt"
			IsAttribute="true"
			Default="4000" />
		<Entry
			id="MaxRoadVertex"
			AssetType="SageInt"
			IsAttribute="true"
			Default="5000" />
		<Entry
			id="MaxRoadIndex"
			AssetType="SageInt"
			IsAttribute="true"
			Default="10000" />
		<Entry
			id="DefaultShroudStatus"
			AssetType="ShroudStatusType"
			IsAttribute="true"
			Default="FOGGED" />
		<Entry
			id="ShroudColor"
			AssetType="RGBColor" />
		<Entry
			id="TreeFadeObjectFilter"
			AssetType="ObjectFilter" />
		<EntryList
			id="StandardPublicBone"
			AssetType="String"
			MinLength="0"
			MaxLength="0" />
		<Entry
			id="UseSimpleHordeDecals"
			AssetType="SageBool"
			IsAttribute="true"
			Default="false" />
		<Entry
			id="FogAlpha"
			AssetType="Byte"
			IsAttribute="true"
			Default="63" />
		<Entry
			id="ShroudAlpha"
			AssetType="Byte"
			IsAttribute="true"
			Default="0" />
		<Entry
			id="ClearAlpha"
			AssetType="Byte"
			IsAttribute="true"
			Default="255" />
	</Asset>

</AssetDefinition>