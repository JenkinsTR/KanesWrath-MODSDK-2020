<?xml version="1.0" encoding="utf-8" ?>
<AssetDefinition xmlns="uri:thundermods.net:SAGE:WrathEdXML:AssetDefinition">

	<Asset
		id="UIComponentTickerGroup">
		<Entry
			id="Name"
			AssetType="String"
			IsAttribute="true"
			IsRequired="true" />
		<Entry
			id="HTTPAddress"
			AssetType="String"
			IsAttribute="true"
			IsRequired="true" />
	</Asset>

	<Asset
		id="UIComponentTicker">
		<EntryInheritance
			id="base"
			AssetType="UIBaseComponent" />
		<Entry
			id="CustomRenderName"
			AssetType="String"
			IsAttribute="true"
			IsRequired="true" />
		<Entry
			id="NumberOfTickerDisplays"
			AssetType="SageInt"
			IsAttribute="true"
			IsRequired="true" />
		<Entry
			id="ScrollSpeedPixelPerSec"
			AssetType="SageReal"
			IsAttribute="true"
			IsRequired="true" />
		<Entry
			id="NumberOfPixelsBetweenHeadline"
			AssetType="SageReal"
			IsAttribute="true"
			IsRequired="true" />
		<Entry
			id="TickerColor"
			AssetType="Color4" />
		<EntryList
			id="TickerGroup"
			AssetType="UIComponentTickerGroup"
			MinLength="0"
			MaxLength="0" />
	</Asset>

</AssetDefinition>