	<GameObject
		id="GDIElfWarrior"
		inheritFrom="BaseInfantry"
		SelectPortrait="Portrait_GDIElfWarrior"
		ButtonImage="Portrait_GDIElfWarrior"
		Side="GDI"
		EditorSorting="UNIT"
		TransportSlotCount="1"
		BuildCost="5000"
		BuildTime="2"
		CommandSet="EmptyCommandSet"
		KindOf="PRELOAD SELECTABLE CAN_ATTACK ATTACK_NEEDS_LINE_OF_SIGHT CAN_CAST_REFLECTIONS INFANTRY PATH_THROUGH_INFANTRY CAN_SHOOT_OVER_WALLS"
		RadarPriority="UNIT"
		ThreatLevel="10"
		ProductionQueueType="INFANTRY"
		UnitCategory="INFANTRY"
		WeaponCategory="BLAST"
		VoicePriority="88"
		EditorName="GDIElfWarrior"
		Description="Desc:GDIElfWarrior"
		TypeDescription="TYPE:GDIElfWarrior">
		<DisplayName
			xai:joinAction="Replace" xmlns:xai="uri:ea.com:eala:asset:instance">NAME:GDIElfWarrior</DisplayName>		
		<ArmorSet
			Armor="GDIElfWarriorArmor"
			DamageFX="InfantryDamageFX" />		
		<LocomotorSet
			Locomotor="HumanLocomotor"
			Condition="NORMAL"
			Speed="65" />
		<SkirmishAIInformation
			UnitBuilderStandardCombatUnit="true" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw"
				OkToChangeModelColor="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="EULorWar_SKN" />					
				</ModelConditionState>
				<xi:include
					href="DATA:Includes/GDIUnitFormationPreviewStates.xml"
					xpointer="xmlns(n=uri:ea.com:eala:asset) xpointer(/n:FormationPreviewStates/child::*)" />
				<!-- invisible when garrisoned -->
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="INSIDE_GARRISON">
					<Model
						Name="" />
					<WeaponLaunchBone
						WeaponSlotType="PRIMARY_WEAPON"
						BoneName="" />
				</ModelConditionState>				
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="INSIDE_GARRISON">
					<!-- no anim -->
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT"
					Flags="RESTART_ANIM_WHEN_COMPLETE"
					AllowRepeatInRandomPick="true"
					StateName="STATE_bored">
					<Animation
						AnimationName="EULorWar_IDLA"
						AnimationMode="ONCE"
						AnimationPriority="50" />
					<Animation
						AnimationName="EULorWar_IDLB"
						AnimationMode="ONCE"
						AnimationPriority="20" />
					<Animation
						AnimationName="EULorWar_IDLC"
						AnimationMode="ONCE"
						AnimationPriority="20" />
					<Animation
						AnimationName="EULorWar_IDLD"
						AnimationMode="ONCE"
						AnimationPriority="25" />
					<Animation
						AnimationName="EULorWar_IDLE"
						AnimationMode="ONCE"
						AnimationPriority="20" />										
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STUNNED_FLAILING"
					StateName="STATE_stunned_flailing">
					<Animation
						AnimationName="EULoRWar_FLYA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					StateName="STATE_stunned_landing">
					<Animation
						AnimationName="EULorWar_LNDA"
						AnimationMode="ONCE" />
				</AnimationState>												
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING">
					<Animation
						AnimationName="EULorWar_DIEA"
						AnimationMode="ONCE" />
					<Animation
						AnimationName="EULorWar_DIEB"
						AnimationMode="ONCE" />
					<Animation
						AnimationName="EULorWar_DIEC"
						AnimationMode="ONCE" />
				</AnimationState>				
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING FIRING_OR_PREATTACK_A">
					<Animation
						AnimationName="EULorWar_RUNC"
						AnimationMode="LOOP" />
					<Animation
						AnimationName="EULorWar_RUND"
						AnimationMode="LOOP" />					
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="FIRING_OR_PREATTACK_A"
					Flags="RESTART_ANIM_WHEN_COMPLETE">
					<Animation
						AnimationName="EULorWar_ATKA"
						AnimationMode="ONCE"
						UseWeaponTiming="true" />
					<Animation
						AnimationName="EULorWar_ATKB"
						AnimationMode="ONCE"
						UseWeaponTiming="true" />
					<Animation
						AnimationName="EULorWar_ATKC"
						AnimationMode="ONCE"
						UseWeaponTiming="true" />					
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="EULorWar_RUNA"
						AnimationMode="LOOP"
						Distance="75" />
					<Animation
						AnimationName="EULorWar_RUNB"
						AnimationMode="LOOP"
						Distance="75" />
				</AnimationState>				
				<!-- CELEBRATING -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="EMOTION_CELEBRATING">
					<Animation
						AnimationName="EULorWar_CHRA"
						AnimationMode="LOOP"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="EULorWar_CHRB"
						AnimationMode="LOOP"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="EULorWar_CHRC"
						AnimationMode="LOOP"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
				</AnimationState>
				<!-- SELECTED -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SELECTED"
					Flags="RESTART_ANIM_WHEN_COMPLETE"
					StateName="STATE_SelectedPose">
					<Animation
						AnimationName="EULorWar_ATNA"
						AnimationMode="ONCE"
						AnimationBlendTime="10" />
					<Animation
						AnimationName="EULorWar_ATNB"
						AnimationMode="ONCE"
						AnimationBlendTime="5" />
					<Animation
						AnimationName="EULorWar_ATNC"
						AnimationMode="ONCE"
						AnimationBlendTime="5" />					
				</AnimationState>								
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<WeaponSetUpdate
				id="ModuleTag_WeaponSetUpdate">
				<WeaponSlotHardpoint
					ID="1">
