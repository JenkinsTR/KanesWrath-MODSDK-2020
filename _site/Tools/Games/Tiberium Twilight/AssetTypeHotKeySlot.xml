<?xml version="1.0" encoding="utf-8" ?>
<AssetDefinition xmlns="uri:thundermods.net:SAGE:WrathEdXML:AssetDefinition">

	<Asset
		id="HotKeySlot">
		<EntryInheritance
			id="base"
			AssetType="BaseInheritableAsset" />
		<Entry
			id="Group"
			AssetType="HotKeySlotGroup"
			IsAttribute="true"
			Default="Misc" />
		<Entry
			id="Index"
			AssetType="SageInt"
			IsAttribute="true"
			Default="-1" />
		<Entry
			id="Name"
			AssetType="String"
			IsAttribute="true"
			IsRequired="true" />
		<Entry
			id="VersionAdded"
			AssetType="SageInt"
			IsAttribute="true"
			Default="1" />
	</Asset>

</AssetDefinition>