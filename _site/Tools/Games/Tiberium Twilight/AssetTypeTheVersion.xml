<?xml version="1.0" encoding="utf-8" ?>
<AssetDefinition xmlns="uri:thundermods.net:SAGE:WrathEdXML:AssetDefinition">

	<Asset
		id="VersionType">
		<Entry
			id="Major"
			AssetType="SageInt"
			IsAttribute="true" />
		<Entry
			id="Minor"
			AssetType="SageInt"
			IsAttribute="true" />
	</Asset>

	<Asset
		id="TheVersion">
		<EntryInheritance
			id="base"
			AssetType="BaseAssetType" />
		<Entry
			id="Version"
			AssetType="VersionType" />
	</Asset>

</AssetDefinition>