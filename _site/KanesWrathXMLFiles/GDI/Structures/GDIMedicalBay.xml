			<AISpecialPowerUpdate
				id="ModuleTag_CommandPostCallAirborneAI"
				CommandButtonName="Command_CommandPostCallAirborne"
				ReinforceDistance="400.0"
				SpecialPowerRadius="300.0"
				SpecialPowerAIType="SPECIAL_POWER_REINFORCEMENT"
			/>

			<ProductionUpdate
				id="ModuleTag_ProductionUpdate"
				Type="UPGRADE"
				GiveNoXP="true" />
			<ParkingPlaceBehavior
				id="ModuleTag_ParkingPlace"
				HealAmount="10"
				NumRows="1"
				NumCols="1"
				ApproachHeight="100"
				CanSetRallyPoint="false"
				ParkInHangars="true">
				<CanParkHereFilter
					Rule="NONE"
					Relationship="SAME_PLAYER"
					Include="SUMMONED TRANSPORT">
					<ExcludeThing>GDIV35Ox_SummonedForVehicle</ExcludeThing>
				</CanParkHereFilter>
			</ParkingPlaceBehavior>
			<!-- Don't allow allies inside -->
			<HealContain
				id="ModuleTag_HordeGarrison"
				ObjectStatusOfContained="UNSELECTABLE ENCLOSED"
				ContainMax="1"
				AllowEnemiesInside="false"
				AllowAlliesInside="false"
				AllowNeutralInside="false"
				AllowOwnPlayerInsideOverride="true"
				ShowPips="false"
				KillPassengersOnDeath="false"
				TimeForFullHeal="5s"
				ResetInitialTeamOnCapture="true">
				<PassengerFilter
					Rule="ANY"
					Include="INFANTRY" />
				<DieMuxData
					DeathTypes="ALL" />
				<PassengerData
					BonePrefix="BONEGARRISON">
					<Filter
						Rule="NONE"
						Include="INFANTRY" />
				</PassengerData>
				<ExitOffset
					x="50"
					y="0"
					z="0" />
			</HealContain>
			<SubObjectsUpgrade
				id="ModuleTag_Upgrade_GDICompositeArmor"
