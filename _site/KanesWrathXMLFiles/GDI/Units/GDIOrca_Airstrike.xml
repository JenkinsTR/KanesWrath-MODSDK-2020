<?xml version="1.0" encoding="utf-8"?>
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xai="uri:ea.com:eala:asset:instance">
	<Tags></Tags>
	<Includes>
		<Include
			type="all"
			source="ART:GUMiniOrca_SKN.w3x" />
		<Include
			type="all"
			source="ART:GUMiniOrca_FLYA.w3x" />
		<Include
			type="all"
			source="ART:GUMiniOrca_IDLA.w3x" />
		<Include
			type="all"
			source="ART:GUMiniOrca_TRNL.w3x" />
		<Include
			type="all"
			source="ART:GUMiniOrca_TRNR.w3x" />
		<Include
			type="all"
			source="ART:GUMiniOrcaR_SKN.w3x" />
		<!-- Base Object -->
		<Include
			type="instance"
			source="DATA:BaseObjects/BaseVehicle.xml" />
	</Includes>
	<GameObject
		id="GDIOrca_Airstrike"
		inheritFrom="BaseVehicle"
		SelectPortrait="Portrait_GDIMiniOrca"
		ButtonImage="Portrait_GDIMiniOrca"
		Side="GDI"
		EditorSorting="UNIT"
		TransportSlotCount="0"
		BuildCost="500"
		BuildTime="5"
		CommandSet="GDIOrcaCommandSet"
		KindOf="PRELOAD CAN_CAST_REFLECTIONS CAN_ATTACK SELECTABLE SCORE AIRCRAFT VEHICLE IGNORES_SELECT_ALL"
		RadarPriority="UNIT"
		ThreatLevel="10"
		ProductionQueueType="AIRCRAFT"
		UnitCategory="AIRCRAFT"
		WeaponCategory="GUN"
		EditorName="GDIOrca_Airstrike"
		Description="Desc:GDIOrca">
		<DisplayName
			xai:joinAction="Replace">Name:GDIOrca</DisplayName>
		<ArmorSet
			Armor="GDIOrcaArmor"
			DamageFX="VehicleDamageFX" />
		<LocomotorSet
			Locomotor="OrcaLocomotor"
			Condition="NORMAL"
			Speed="200.0" />
		<LocomotorSet
			Locomotor="OrcaLocomotor"
			Condition="SUPERSONIC"
			Speed="280.0" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw"
				OkToChangeModelColor="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="GUMiniOrca_SKN" />
					<WeaponLaunchBone
						WeaponSlotID="1"
						WeaponSlotType="PRIMARY_WEAPON"
						BoneName="FX_Weapon_" />
					<ParticleSysBone
						BoneName="exhaust01"
						FXParticleSystemTemplate="GDIOrcaUpgrade"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="exhaust02"
						FXParticleSystemTemplate="GDIOrcaUpgrade"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="exhaust01"
						FXParticleSystemTemplate="GDIOrcaUpgrade02"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="exhaust02"
						FXParticleSystemTemplate="GDIOrcaUpgrade02"
						FollowBone="true" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DAMAGED">
					<Model
						Name="GUMiniOrca_SKN" />
					<Texture
						Original="GUMiniOrca"
						New="GUMiniOrca_D" />
					<ParticleSysBone
						BoneName="exhaust01"
						FXParticleSystemTemplate="GDIOrcaUpgrade"
						FollowBone="true" />

					<ParticleSysBone
						BoneName="exhaust01"
						FXParticleSystemTemplate="GDIOrcaUpgrade02"
						FollowBone="true" />

					<ParticleSysBone
						BoneName="exhaust02"
						FXParticleSystemTemplate="orcadamagesmoke"
						FollowBone="true" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="REALLYDAMAGED">
					<Model
						Name="GUMiniOrca_SKN" />
					<Texture
						Original="GUMiniOrca"
						New="GUMiniOrca_D" />
					
					<ParticleSysBone
						BoneName="exhaust01"
						FXParticleSystemTemplate="orcaexhaust"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="exhaust02"
						FXParticleSystemTemplate="orcaexhaust"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="exhaust02"
						FXParticleSystemTemplate="orcadamagefire"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="exhaust02"
						FXParticleSystemTemplate="orcadamagesmoke"
						FollowBone="true" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING">
					<Model
						Name="GUMiniOrcaR_SKN" />
					<ParticleSysBone
						BoneName="ROOTTRANSFORM"
						FXParticleSystemTemplate="GDIDebrisSmallFire"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="ROOTTRANSFORM"
						FXParticleSystemTemplate="GDIDebrisSmallFireDistortion"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="ROOTTRANSFORM"
						FXParticleSystemTemplate="GDIDebrisSmallSmoke"
						FollowBone="true" />
				</ModelConditionState>
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT">
					<Animation
						AnimationName="GUMiniOrca_IDLA"
						AnimationMode="LOOP"
						AnimationSpeedFactorMin="0.9"
						AnimationSpeedFactorMax="1.1" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="LOCOMOTOR_TAXIING">
					<Animation
						AnimationName="GUMiniOrca_IDLA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING TURN_LEFT_HIGH_SPEED">
					<Animation
						AnimationName="GUMiniOrca_TRNL"
						AnimationMode="ONCE"
						AnimationBlendTime="15" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING TURN_RIGHT_HIGH_SPEED">
					<Animation
						AnimationName="GUMiniOrca_TRNR"
						AnimationMode="ONCE"
						AnimationBlendTime="15" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING TURN_LEFT">
					<Animation
						AnimationName="GUMiniOrca_TRNL"
						AnimationMode="ONCE"
						AnimationBlendTime="15" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING TURN_RIGHT">
					<Animation
						AnimationName="GUMiniOrca_TRNR"
						AnimationMode="ONCE"
						AnimationBlendTime="15" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="ACCELERATE">
					<Animation
						AnimationName="GUMiniOrca_FLYA"
						AnimationMode="ONCE"
						AnimationBlendTime="15" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DECELERATE">
					<Animation
						AnimationName="GUMiniOrca_FLYA"
						AnimationMode="ONCE"
						AnimationBlendTime="15" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING">
					<Animation
						AnimationName="GUMiniOrca_FLYA"
						AnimationMode="LOOP"
						AnimationBlendTime="15" />
				</AnimationState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
      <ModelConditionUpgrade
				id="ModuleTag_ShowThePlating"
				Permanent="true"
				AddConditionFlags="USER_1">
        <TriggeredBy>Upgrade_GDIFaction</TriggeredBy>
      </ModelConditionUpgrade>
      <ModelConditionUpgrade
				id="ModuleTag_ShowThePlatingSteelTalons"
				Permanent="true"
				AddConditionFlags="USER_1">
        <TriggeredBy>Upgrade_SteelTalonsFaction</TriggeredBy>
      </ModelConditionUpgrade>
      <ModelConditionUpgrade
				id="ModuleTag_ShowThePlatingZOCOM"
				Permanent="true"
				AddConditionFlags="USER_1">
        <TriggeredBy>Upgrade_ZOCOMFaction</TriggeredBy>
      </ModelConditionUpgrade>
      <StrafeAreaUpdate
				id="ModuleTag_StrafeAreaUpdate"
				WeaponName="GDIAirstrikeOrcaWeapon"
				Radius="40"
				SweepFrequency="0.2"
				SweepAmplitude="20" />
			<!-- gravity mult of 30 is temporary -->
			<Physics
				id="ModuleTag_07"
				GravityMultiplier="30" />
			<SubObjectsUpgrade
				id="ModuleTag_HideAll"
				HideSubObjects="UG_EMP UG_HELLFIRE UG_ENGINE UG_GUNS">
				<TriggeredBy>Upgrade_ObjectDefault</TriggeredBy>
			</SubObjectsUpgrade>
			<SlowDeath
				id="ModuleTag_Death"
				SinkDelay="0s"
				SinkRate="0.5"
				DestructionDelay="0.01s">
				<OCL
					Type="INITIAL">
					<OCL>OCL_GDIMiniOrcaDebris</OCL>
				</OCL>
				<DieMuxData
					DeathTypes="ALL"
					DeathTypesForbidden="TOPPLED" />
			</SlowDeath>
			<DestroyDie
				id="ModuleTag_DeathFaded">
				<DieMuxData
					DeathTypes="FADED" />
			</DestroyDie>
			<FXListBehavior
				id="ModuleTag_FXList">
				<DieMuxData
					DeathTypes="ALL" />
				<Event
					Index="onDeath"
					FX="FX_GDIAirVehicleExplode" />
			</FXListBehavior>
			<!-- No stances because it's summoned -->
			<StancesBehavior
				id="ModuleTag_Stance"
				xai:joinAction="Remove" />
		</Behaviors>
		<AI>
			<JetAIUpdate
				id="ModuleTag_JetAIUpdate"
				AutoAcquireEnemiesWhenIdle="NO"
				NeedsRunway="false"
				AILuaEventsList="GDIOrcaAirstrikeEventsList">
				<UnitAITargetChooserData
					SympathyRange="100.0" />
				<LockonInfo />
			</JetAIUpdate>
		</AI>
		<Body>
			<ActiveBody
				id="ModuleTag_04"
				MaxHealth="3000" />
		</Body>
		<Geometry
			IsSmall="false">
			<Shape
				Type="BOX"
				MajorRadius="20.0"
				MinorRadius="3.0"
				Height="25.0"></Shape>
		</Geometry>
		<AudioArrayVoice>
			<!-- VoiceAttack				= TEMP_OrcaVoiceSelect
			VoiceGuard				= TEMP_OrcaVoiceSelect
			VoiceMove				= TEMP_OrcaVoiceSelect
			VoiceSelect				= TEMP_OrcaVoiceSelect

			SoundAmbient			= TEMP_OrcaAmbientLoop
			SoundMoveStart			= OrcaMoveStartMS
			SoundAmbientRubble		= NoSound
			-->
		</AudioArrayVoice>
		<ShadowInfo
			Type="VOLUME" />
		<VisionInfo
			VisionRange="180"
			ShroudClearingRange="600" />
	</GameObject>
</AssetDeclaration>