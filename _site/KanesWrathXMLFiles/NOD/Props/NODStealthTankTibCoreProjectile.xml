<?xml version="1.0" encoding="us-ascii"?>
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xai="uri:ea.com:eala:asset:instance">
	<Tags></Tags>
	<Includes>
		<Include
			type="all"
			source="ART:NPMissile.w3x" />
		<Include
			type="all"
			source="ART:FX_RPGHeroic.w3x" />			
	</Includes>
	<GameObject
		id="NODStealthTankTibCoreProjectile"
		EditorSorting="SYSTEM"
		KindOf="PROJECTILE SMALL_MISSILE DEFLECT_BY_SPECIAL_POWER NO_COLLIDE HIDE_IF_FOGGED"
		EditorName="NODStealthTankTibCoreProjectile">
		<ArmorSet
			Armor="NoArmor" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_ScriptedModelDraw">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="NPMissile" />
					<ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="TibCoreMissileProjectileTrail"
						FollowBone="true" />
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<FXListBehavior
				id="ModuleTag_FXList">
				<DieMuxData
					DeathTypes="ALL" />
				<Event
					Index="onDeath"
					FX="FX_NOD_StealthTank_RocketImpactExplosion" />
			</FXListBehavior>
			<DestroyDie
				id="ModuleTag_Die">
				<DieMuxData
					DeathTypes="ALL" />
			</DestroyDie>
			<BezierProjectile
				id="ModuleTag_BezierProjectileBehavior"
				FirstHeightMin="10"
				FirstHeightMax="25"
				SecondHeightMin="30"
				SecondHeightMax="40"
				FirstPercentIndent="20"
				SecondPercentIndent="90"
				GroundHitFX="FX_NOD_StealthTank_TibCoreMissileMiss"
				DetonateCallsKill="true" 
				MaxDistanceReachedFX="FX_GDIMissleSmallHit"
				CurveFlattenMinDist="300"
				SidewaysDrift="20"
				OrientToFlightPath="true"
				FlightPathAdjustDistPerSecond="100"/>
		</Behaviors>
		<Body>
			<ActiveBody
				id="ModuleTag_ActiveBody"
				MaxHealth="100.0" />
		</Body>
		<Geometry
			IsSmall="true">
			<Shape
				Type="SPHERE"
				MajorRadius="0.8"></Shape>
		</Geometry>
		<AudioArraySound>
			<AudioEntry Sound="NOD_StealthTank_RocketZipBy" AudioType="soundAmbient" />
		</AudioArraySound>
		<VisionInfo
			VisionRange="0" />
	</GameObject>
	
	<GameObject
		inheritFrom="NODStealthTankTibCoreProjectile"
		id="NODStealthTankTibCoreProjectile_Veteran"
		EditorName="NODStealthTankTibCoreProjectile_Veteran">
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_ScriptedModelDraw"
				xai:joinAction="Replace">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="FX_RPGHeroic" />
				<ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="TibCoreMissileProjectileTrail"
						FollowBone="true" />	
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<BezierProjectile
				id="ModuleTag_BezierProjectileBehavior"
				FirstHeightMin="10"
				FirstHeightMax="25"
				SecondHeightMin="30"
				SecondHeightMax="40"
				FirstPercentIndent="20"
				SecondPercentIndent="90"
				GroundHitFX="FX_GDIMissleMissHeroic"
				DetonateCallsKill="true" 
				MaxDistanceReachedFX=""
				CurveFlattenMinDist="300"
				SidewaysDrift="20"
				OrientToFlightPath="true"
				FlightPathAdjustDistPerSecond="100"/>
		</Behaviors>		
	</GameObject>	
	
</AssetDeclaration>
