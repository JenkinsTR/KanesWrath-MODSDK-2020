					<Script> 		CurDrawableShowSubObject("LASER") 		CurDrawableHideSubObject("GUN_BUTT")					 		Prev = CurDrawablePrevAnimationState() 		if Prev == "STATE_Cover" then 		CurDrawableSetTransitionAnimState("TRANS_fromCoverGunHolstered") 		end 	</Script>
				</AnimationState>
				<AnimationState 	
					ParseCondStateType="PARSE_NORMAL" 	
					ConditionsYes="COVER"
					Flags="RESTART_ANIM_WHEN_COMPLETE" 	
					StateName="STATE_Cover">
					<Animation 		
						AnimationName="GUEngineer_KBIA" 		
						AnimationMode="ONCE"
						AnimationPriority="15"
						AnimationBlendTime="5"
						AnimationMustCompleteBlend="true" />
					<Animation 	  
						AnimationName="GUEngineer_KBIA" 	 
						AnimationMode="ONCE" 	  
						AnimationPriority="15" 	  
						AnimationBlendTime="5" 	 
						AnimationMustCompleteBlend="true" />
					<Animation 	  
						AnimationName="GUEngineer_KBFA" 	  
						AnimationMode="ONCE" 	  
						AnimationPriority="7" 	  
						AnimationBlendTime="5" 	  
						AnimationMustCompleteBlend="true" />
					<Animation 	  
						AnimationName="GUEngineer_KBFB" 	  
						AnimationMode="ONCE" 	  
						AnimationPriority="5" 	  
						AnimationBlendTime="5" 	  
						AnimationMustCompleteBlend="true" />
					<Animation 	 
						AnimationName="GUEngineer_KBFC" 	 
						AnimationMode="ONCE" 	 
						AnimationPriority="7" 	 
						AnimationBlendTime="5" 	 
						AnimationMustCompleteBlend="true" />
					<Script> 		
						Prev = CurDrawablePrevAnimationState() 		
						if Prev == "STATE_CoverFiring" then 		
							CurDrawableSetTransitionAnimState("TRANS_toCoverGunHolstered") 		
						end 	
					</Script>
				</AnimationState>
				<AnimationState 	ParseCondStateType="PARSE_NORMAL" 	ConditionsYes="MOVING USING_WEAPON_A">
					<Animation 		AnimationName="GUCEngineer_ATRA" 		AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState 	ParseCondStateType="PARSE_NORMAL" 	ConditionsYes="FIRING_OR_PREATTACK_A" 	StateName="STATE_firing">
