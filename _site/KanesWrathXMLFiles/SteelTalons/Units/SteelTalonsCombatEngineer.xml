					<Animation 		AnimationName="GUEngineer_SIDA" 		AnimationMode="ONCE" />
					<Animation 		AnimationName="GUEngineer_SIFA" 		AnimationMode="ONCE" />
					<Animation 	  AnimationName="GUEngineer_SIFB" 	  AnimationMode="ONCE" />
					<Script> 		Prev = CurDrawablePrevAnimationState() 		if Prev ~= "STATE_Suppressed" and Prev ~= "TRANS_toSuppressed" and Prev ~= "STATE_SuppressedFiring" then 		CurDrawableSetTransitionAnimState("TRANS_toSuppressed") 		if Prev == "STATE_SuppressedFiring" then 		CurDrawableSetTransitionAnimState("TRANS_toSuppressedGunHolstered") 		end 	</Script>
				</AnimationState>
				<!--Cover-->
				<AnimationState 	ParseCondStateType="PARSE_NORMAL" 	ConditionsYes="COVER MOVING" 	Flags="RANDOMSTART" 	StateName="STATE_Cover">
					<Animation 		AnimationName="GUEngineer_CRNA" 		AnimationMode="LOOP" 		Distance="75" />
				</AnimationState>
				<AnimationState 	ParseCondStateType="PARSE_NORMAL" 	ConditionsYes="SELECTED COVER" 	StateName="STATE_Cover">
					<Animation 		AnimationName="GUEngineer_KAIA" 		AnimationMode="LOOP" />
				</AnimationState>
