					<Script> 		CurDrawableShowSubObject("LASER") 		CurDrawableHideSubObject("GUN_BUTT")					 		Prev = CurDrawablePrevAnimationState() 		if Prev == "STATE_Bored" then 		CurDrawableSetTransitionAnimState("TRANS_fromStandingGunHolstered") 		end 		if Prev == "STATE_Selected" then 		CurDrawableSetTransitionAnimState("TRANS_fromStandingGunHolstered") 		end 	</Script>
				</AnimationState>
				<AnimationState 	ParseCondStateType="PARSE_NORMAL" 	ConditionsYes="MOVING" 	Flags="RANDOMSTART">
					<Animation 		AnimationName="GUCEngineer_RUNB1" 		AnimationMode="LOOP" 		AnimationSpeedFactorMin="1.0" 		AnimationSpeedFactorMax="1.0" />
				</AnimationState>
				<!-- EMOTIONS -->
				<AnimationState 	ParseCondStateType="PARSE_NORMAL" 	ConditionsYes="EMOTION_CELEBRATING" 	Flags="RESTART_ANIM_WHEN_COMPLETE">
