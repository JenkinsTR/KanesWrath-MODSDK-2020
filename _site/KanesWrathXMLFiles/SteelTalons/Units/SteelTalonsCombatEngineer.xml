					<Animation 		AnimationName="GUCEngineer_KAKA3" 		AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState 			ParseCondStateType="PARSE_TRANSITION" 			StateName="TRANS_fromSuppressedGunHolstered">
