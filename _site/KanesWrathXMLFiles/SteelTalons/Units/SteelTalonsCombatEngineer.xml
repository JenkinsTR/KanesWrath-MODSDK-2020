					ParseCondStateType="PARSE_DEFAULT" 	
					Flags="RESTART_ANIM_WHEN_COMPLETE" 	
					AllowRepeatInRandomPick="true" 	
					StateName="STATE_Bored">
					<Animation 		
						AnimationName="GUEngineer_BIDA" 		
						AnimationMode="ONCE" 		
						AnimationPriority="10" />
					<Animation 		
						AnimationName="GUEngineer_BIFA" 		
						AnimationMode="ONCE" 		
						AnimationPriority="1" />
					<Animation 				  
						AnimationName="GUEngineer_BIFB" 				  
						AnimationMode="ONCE" 				  
						AnimationPriority="5" />
					<Animation 				  
						AnimationName="GUCEngineer_BIFZ" 				  
						AnimationMode="ONCE" 				  
						AnimationPriority="8" />
					<Animation 				  
						AnimationName="GUEngineer_BIFD" 				  
						AnimationMode="ONCE" 				  
						AnimationPriority="1" />
					<Script> 					 		
						CurDrawableHideSubObject("LASER") 		
						CurDrawableShowSubObject("GUN_BUTT")					 	 		
						Prev = CurDrawablePrevAnimationState() 		
						if Prev == "STATE_Suppressed" then 		
							CurDrawableSetTransitionAnimState("TRANS_fromSuppressed") 		
						end 		
						if Prev == "STATE_Selected" then 		
							CurDrawableSetTransitionAnimState("TRANS_attentionToBored") 		
						end 		
						if Prev == "STATE_stunned_flailing" then 		
							CurDrawableSetTransitionAnimState("STATE_stunned_landing") 		
						end 		
						if Prev == "STATE_firing" then 		
							CurDrawableSetTransitionAnimState("TRANS_toStandingGunHolstered") 		
						end 	
				</Script>
				</AnimationState>
				<AnimationState 	ParseCondStateType="PARSE_NORMAL" 	ConditionsYes="STUNNED_FLAILING" 	StateName="STATE_stunned_flailing">
					<Animation 		AnimationName="GUEngineer_FLYA" 		AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState 	ParseCondStateType="PARSE_NORMAL" 	StateName="STATE_stunned_landing">
					<Animation 		AnimationName="GUEngineer_LNDA" 		AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState 	ParseCondStateType="PARSE_NORMAL" 	ConditionsYes="FRONTCRUSHED">
