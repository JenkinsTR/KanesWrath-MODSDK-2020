					<Animation 	  AnimationName="GUEngineer_AIFB" 	  AnimationMode="ONCE" 	  AnimationPriority="5" 	  AnimationBlendTime="5" 	  AnimationMustCompleteBlend="true" />
					<Animation 	  AnimationName="GUEngineer_AIFC" 	  AnimationMode="ONCE" 	  AnimationPriority="2" 	  AnimationBlendTime="5" 	  AnimationMustCompleteBlend="true" />
					<Script> 		CurDrawableHideSubObject("LASER") 		CurDrawableShowSubObject("GUN_BUTT")	 		Prev = CurDrawablePrevAnimationState() 		if Prev == "STATE_Suppressed" then 		CurDrawableSetTransitionAnimState("TRANS_fromSuppressedB") 		end 		if Prev == "STATE_Bored" then 		CurDrawableSetTransitionAnimState("TRANS_boredToAttention") 		end 		if Prev == "STATE_firing" then 		CurDrawableSetTransitionAnimState("TRANS_toStandingGunHolstered") 		end 	</Script>
				</AnimationState>
				<!-- TRANSITIONS -->
				<AnimationState 	ParseCondStateType="PARSE_TRANSITION" 	StateName="TRANS_boredToAttention">
