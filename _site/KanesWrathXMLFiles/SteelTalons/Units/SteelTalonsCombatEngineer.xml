					<Script> 		CurDrawableShowSubObject("LASER") 		CurDrawableHideSubObject("GUN_BUTT")					 		Prev = CurDrawablePrevAnimationState() 		if Prev == "STATE_Suppressed" then 		CurDrawableSetTransitionAnimState("TRANS_fromSuppressedGunHolstered") 		end 	</Script>
				</AnimationState>
				<AnimationState 	ParseCondStateType="PARSE_NORMAL" 	ConditionsYes="SUPPRESSED MOVING" 	StateName="STATE_Suppressed" 	Flags="RANDOMSTART">
					<Animation 		AnimationName="GUEngineer_SMVA" 		AnimationMode="LOOP" />
				</AnimationState>
