						AnimationName="AUMstrmind_ATKB"
						AnimationMode="ONCE"
						UseWeaponTiming="true" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="FIRING_OR_PREATTACK_A"
					FrameForPristineBonePositions="5">
					<Animation
						AnimationName="AUMstrmind_ATKA"
						AnimationMode="ONCE"
						UseWeaponTiming="true" />
