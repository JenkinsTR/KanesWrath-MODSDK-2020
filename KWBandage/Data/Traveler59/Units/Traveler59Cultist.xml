					AnimationMode="ONCE" /> 
				<Animation 
					AnimationName="AUCultist_FDIC" 
					AnimationMode="ONCE" />
			</AnimationState> 
			<AnimationState 
				ParseCondStateType="PARSE_NORMAL" 
				ConditionsYes="MOVING FIRING_OR_PREATTACK_A" 
				StateName="STATE_Run" 
				Flags="RANDOMSTART"> 
				<Animation 
					AnimationName="AUCultist_RUNB" 
					AnimationMode="LOOP"
					AnimationBlendTime="5" 
					Distance="80" />
				</AnimationState>
			<AnimationState 
				ParseCondStateType="PARSE_NORMAL" 
				ConditionsYes="MOVING" 
				StateName="STATE_Run" 
				Flags="RANDOMSTART">
				<Animation 
					AnimationName="AUCultist_RUNA" 
					AnimationMode="LOOP" 
					Distance="80" 
					AnimationPriority="30"
            		AnimationBlendTime="10"
            		AnimationSpeedFactorMin="3" 
					AnimationSpeedFactorMax="3"/>
				<Animation 
					AnimationName="AUCultist_RUNB" 
					AnimationMode="LOOP"
					Distance="80" 
					AnimationPriority="25"
            		AnimationBlendTime="5"
            		AnimationSpeedFactorMin="3"
					AnimationSpeedFactorMax="3"/>
				<Animation 	
					AnimationName="AUCultist_RUNC" 
					AnimationMode="LOOP" 
					Distance="80" 
					AnimationPriority="20"
            		AnimationBlendTime="5"
            		AnimationSpeedFactorMin="3" 
					AnimationSpeedFactorMax="3"/>
				<Script>
					Prev = CurDrawablePrevAnimationState()
					if Prev == "STATE_Cover" then CurDrawableSetTransitionAnimState("TRANS_CoverToRun")
					end
				</Script>
			</AnimationState> 
			<AnimationState 
				ParseCondStateType="PARSE_NORMAL" 
				ConditionsYes="FIRING_OR_PREATTACK_A" 
				FrameForPristineBonePositions="5"> 
				<Animation 
					AnimationName="AUCultist_ATKA" 
					AnimationMode="ONCE" 
					UseWeaponTiming="true"
					AnimationBlendTime="10" />
			</AnimationState>
			<AnimationState 
				ParseCondStateType="PARSE_NORMAL" 
				ConditionsYes="ATTACKING">
				<Animation 
				AnimationName="AUCultist_ATKA" 
				AnimationMode="MANUAL"
				AnimationBlendTime="10" />
			</AnimationState> 
			<!-- SELECTED --> 
			<AnimationState 
				ParseCondStateType="PARSE_NORMAL" 
				ConditionsYes="SELECTED" 
				Flags="RESTART_ANIM_WHEN_COMPLETE" 
				StateName="STATE_Selected"> 
				<Animation 
					AnimNickName="ATNB" 
					AnimationName="AUCultist_AIDA" 
					AnimationMode="ONCE"
					AnimationPriority="80" />
          <!--  AnimationBlendTime="5" --> 	
				<Animation 
					AnimNickName="ATNB" 
					AnimationName="AUCultist_AIDB" 
					AnimationMode="ONCE"
					AnimationPriority="40" /> 	
				<Animation 
					AnimNickName="ATNB" 
					AnimationName="AUCultist_AIFA" 
					AnimationMode="ONCE"
					AnimationPriority="20" />
