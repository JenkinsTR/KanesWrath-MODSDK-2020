			<AnimationState
				ParseCondStateType="PARSE_DEFAULT" 
				StateName="STATE_bored" 
				Flags="RESTART_ANIM_WHEN_COMPLETE"> 
				<Animation 	
					AnimationName="AUCultist_BIDA" 	
					AnimationMode="ONCE" 
					AnimationPriority="80" 
					AnimationBlendTime="15" />
				<Animation 
					AnimationName="AUCultist_BIDB" 
					AnimationMode="ONCE" 
					AnimationPriority="80" 
					AnimationBlendTime="15" /> 
				<Animation 
					AnimationName="AUCultist_BIDC" 
					AnimationMode="ONCE" 
					AnimationPriority="80" 
					AnimationBlendTime="15" /> 
				<Animation 
					AnimationName="AUCultist_BIFA" 
					AnimationMode="ONCE" 
					AnimationPriority="40" 
					AnimationBlendTime="5" /> 
				<Animation 
					AnimationName="AUCultist_BIFB" 
					AnimationMode="ONCE" 
					AnimationPriority="40" 
					AnimationBlendTime="5" /> 
				<Animation 
					AnimationName="AUCultist_BIFC" 
					AnimationMode="ONCE" 
					AnimationPriority="40" 
					AnimationBlendTime="5" /> 
				<Animation 
					AnimationName="AUCultist_BIFD" 
					AnimationMode="ONCE" 
					AnimationPriority="40" 
					AnimationBlendTime="5" />
				<Script> 
					Prev = CurDrawablePrevAnimationState();
					if Prev == "STATE_Selected" or Prev == "ATNB" or Prev == "STATE_Run" then 
					CurDrawableSetTransitionAnimState("TRANS_SelectedToIdle") 
					end; 
				</Script>
			</AnimationState> 
			<AnimationState 
				ParseCondStateType="PARSE_NORMAL" 
				ConditionsYes="FRONTCRUSHED"> 
				<Animation 
					AnimationName="AUCultist_CDTA" 
					AnimationMode="ONCE" /> 
			</AnimationState> 
			<AnimationState 
				ParseCondStateType="PARSE_NORMAL" 
				ConditionsYes="BACKCRUSHED"> 
				<Animation 
					AnimationName="AUCultist_CDTB" 
					AnimationMode="ONCE" /> 
			</AnimationState> 
			<AnimationState 
				ParseCondStateType="PARSE_NORMAL" 
				ConditionsYes="DYING"> 
				<Animation 
					AnimationName="AUCultist_DIEA" 
					AnimationMode="ONCE" />
				<Animation 
					AnimationName="AUCultist_DIEB" 
					AnimationMode="ONCE" />
				<Animation 
					AnimationName="AUCultist_FDIA" 
					AnimationMode="ONCE" />
				<Animation 
