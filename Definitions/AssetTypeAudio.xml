		<Entry>VOICE</Entry>
		<Entry>MUSIC</Entry>
		<Entry>AMBIENT</Entry>
		<Entry>MOVIE</Entry>
		<Entry>NONE</Entry>
	</EnumAsset>
	
	<Asset id="AudioVolumeSliderMultiplier">
