<?xml version="1.0" encoding="us-ascii"?>
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xai="uri:ea.com:eala:asset:instance">
  <Tags></Tags>
  <Includes>
    <Include
      type="all"
      source="DATA:GlobalData/GlobalDefines.xml" />

    <Include type="all" source="ART:MGStrike_SCR04.w3x" />
    <Include type="all" source="ART:MG_Strike_Logo_Scrin.xml" />
    <Include type="all" source="ART:MG_Strike_Logo_Traveler.xml" />
    <!-- Base Object -->
    <Include type="instance" source="DATA:MetaGame/units_and_buildings/Alien/AlienStrikeForceUltraHeavy.xml" />
  </Includes>
  <GameObject
		id="T59StrikeForceUltraHeavy"
		inheritFrom="AlienStrikeForceUltraHeavy">
    <Draws>
      <TankDraw
        id="ModuleTag_Draw"
				xai:joinAction="Replace"
  			OkToChangeModelColor="true"
        NoRotate="true">
        <ModelConditionState
					ParseCondStateType="PARSE_DEFAULT"
					RetainSubObjects="true">
          <Texture
				  Original="MG_Strike_Logo_Scrin"
				  New="MG_Strike_Logo_Traveler" />
          <Model
						Name="MGStrike_SCR04" />
        </ModelConditionState>
        <AnimationState
					ParseCondStateType="PARSE_DEFAULT">
          <Script>
            CurDrawableHideSubObject("AOE")
          </Script>
        </AnimationState>
        <AnimationState
					ParseCondStateType="PARSE_NORMAL"
          ConditionsYes="SELECTED">
          <Script>
            CurDrawableShowSubObject("AOE")
          </Script>
        </AnimationState>
      </TankDraw>
    </Draws>
   </GameObject>
</AssetDeclaration>
