<?xml version="1.0" encoding="us-ascii"?>
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xai="uri:ea.com:eala:asset:instance">
	<Tags></Tags>
	<Includes>
		<Include
		  type="instance"
		  source="DATA:BaseObjects/BaseCrushableObstacle.xml" />
	</Includes>
	<GameObject
		id="BaseParkedCar"
		inheritFrom="BaseCrushableObstacle" 
		EditorSorting="MISC_MAN_MADE"
		EditorName="BaseParkedCar">
		<DisplayName>Prop</DisplayName>
		<Behaviors>
			<SquishCollide
				id ="ModuleTag_SquishCollide"
				UseDirectionCheck="false"/>
			<Physics
				id="ModuleTag_Physics"/>
			<CrushDie
				id="ModuleTag_CrushDie">
				<DieMuxData
					DeathTypes="ALL" />
			</CrushDie>
			<SlowDeath
				id="ModuleTag_Death"
				xai:joinAction="Overwrite" 
				SinkDelay="0s"
				SinkRate="3"
				DestructionDelay="4s">
				<DieMuxData
					DeathTypes="ALL" />
			</SlowDeath>
			<FXListBehavior
				id="ModuleTag_FXList">
				<DieMuxData
					DeathTypes="ALL" />
				<Event
					Index="onDeath"
					FX="FX_CarsExplode" />
			</FXListBehavior>
			<ToppleUpdate
				id="ModuleTag_Topple"
				xai:joinAction="Remove"/>
		</Behaviors>
	</GameObject>
</AssetDeclaration>