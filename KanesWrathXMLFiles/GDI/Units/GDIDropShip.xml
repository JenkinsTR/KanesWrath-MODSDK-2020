				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DECELERATE">
					<Animation
						AnimationName="GUDropShip_DECL"
						AnimationMode="ONCE"
						AnimationBlendTime="15" />
