					<Model
						Name="GUDropShip_SKN" />
				</ModelConditionState>
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="GUDropShip_IDLA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DOOR_1_OPENING">
					<Animation
						AnimationName="GUDropShip_OPNA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DOOR_1_CLOSING">
					<Animation
						AnimationName="GUDropShip_CLSA"
						AnimationMode="ONCE" />
				</AnimationState>
				
				
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
