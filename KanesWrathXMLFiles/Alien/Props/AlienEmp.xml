<?xml version="1.0" encoding="us-ascii"?>
<!-- The default EMP projectile -->
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Tags></Tags>
	<Includes>
		
	</Includes>
	<GameObject
		id="AlienEMP"
		Side="ALIEN"
		EditorSorting="SYSTEM"
		KindOf="PROJECTILE NO_COLLIDE HIDE_IF_FOGGED"
		EditorName="AlienEMP">
		<ArmorSet
			Armor="NoArmor" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_ScriptedModelDraw">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="" />
					<ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="EmpPulse"
						FollowBone="true" />
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>

			<FXListBehavior
				id="ModuleTag_FXLists">
				<DieMuxData
					DeathTypes="ALL" />
				<Event
					Index="onDeath"
					FX="FX_Alien_EMPImpact" />
			</FXListBehavior>

			<DestroyDie
				id="ModuleTag_Die">
				<DieMuxData
					DeathTypes="ALL" />
			</DestroyDie>
			<BezierProjectile
				id="ModuleTag_BezierProjectileBehavior"
				FirstHeightMin="0"
				FirstHeightMax="0"
				SecondHeightMin="0"
				SecondHeightMax="0"
				FirstPercentIndent="30"
				SecondPercentIndent="90"
				CurveFlattenMinDist="100"
				DetonateCallsKill="true" 
				FlightPathAdjustDistPerSecond="100"
				OrientToFlightPath="true">
			</BezierProjectile>
		</Behaviors>
		<Body>
			<ActiveBody
				id="ModuleTag_ActiveBody"
				MaxHealth="100.0" />
		</Body>
		<Geometry
			IsSmall="true">
			<Shape
				Type="SPHERE"
				MajorRadius="0.8"></Shape>
		</Geometry>
		<VisionInfo
			VisionRange="0" />
	</GameObject>
</AssetDeclaration>