<?xml version="1.0" encoding="us-ascii"?>
<!-- The default plasma disk projectile -->
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xai="uri:ea.com:eala:asset:instance">
	<Tags></Tags>
	<Includes>
		<Include
			type="all"
			source="ART:EXDisk.w3x" />
	</Includes>
	<GameObject
		id="AlienPlasmaDisk"
		Side="ALIEN"
		EditorSorting="SYSTEM"
		KindOf="PROJECTILE NO_COLLIDE HIDE_IF_FOGGED"
		EditorName="AlienPlasmaDisk">
		<ArmorSet
			Armor="NoArmor" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_ScriptedModelDraw">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="EXDisk" />
					<ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="AlienPlasmaDiskTrail"
						FollowBone="true" />
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<FXListBehavior	
				id="ModuleTag_FXLists">
				<DieMuxData	
					DeathTypes="ALL"/>
				<Event 
					Index="onDeath" 
					FX="FX_ALI_SeekerTank_WeaponProjectileImpact" />
			</FXListBehavior>
			<DestroyDie
				id="ModuleTag_Die">
				<DieMuxData
					DeathTypes="ALL" />
			</DestroyDie>
			<BezierProjectile
				id="ModuleTag_BezierProjectileBehavior"
				FirstHeightMin="20"
				FirstHeightMax="20"
				SecondHeightMin="20"
				SecondHeightMax="20"
				FirstPercentIndent="30"
				SecondPercentIndent="90"
				CurveFlattenMinDist="100"
				SidewaysDrift="50"
				PingPongSidewaysDrift="false"
				DetonateCallsKill="true" 
				FlightPathAdjustDistPerSecond="100"
				GroundHitFX="FX_PlasmaDiskGroundHit"
				OrientToFlightPath="true">
			</BezierProjectile>
		</Behaviors>
		<Body>
			<ActiveBody
				id="ModuleTag_ActiveBody"
				MaxHealth="100.0" />
		</Body>
		<Geometry
			IsSmall="true">
			<Shape
				Type="SPHERE"
				MajorRadius="0.8"></Shape>
		</Geometry>
		<VisionInfo
			VisionRange="0" />
	</GameObject>
	
	
	<GameObject
		inheritFrom="AlienPlasmaDisk"
		id="AlienPlasmaDisk_Veteran"
		EditorName="AlienPlasmaDisk_Veteran">
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw"
				xai:joinAction="Replace">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="EXDisk" />
					<ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="AlienPlasmaDiskTrailHeroic"
						FollowBone="true" />
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<FXListBehavior
				id="ModuleTag_FXList"
				xai:joinAction="Replace">
				<DieMuxData
					DeathTypes="ALL" />
				<Event
					Index="onDeath"
					FX="FX_ALI_SeekerTank_WeaponProjectileImpact" />
			</FXListBehavior>
			<BezierProjectile
				id="ModuleTag_BezierProjectileBehavior"
				FirstHeightMin="20"
				FirstHeightMax="20"
				SecondHeightMin="20"
				SecondHeightMax="20"
				FirstPercentIndent="30"
				SecondPercentIndent="90"
				CurveFlattenMinDist="100"
				SidewaysDrift="50"
				PingPongSidewaysDrift="false"
				DetonateCallsKill="true" 
				FlightPathAdjustDistPerSecond="100"
				GroundHitFX="FX_PlasmaDiskGroundHitHeroic"
				OrientToFlightPath="true">
			</BezierProjectile>			
		</Behaviors>
	</GameObject>	
	
</AssetDeclaration>