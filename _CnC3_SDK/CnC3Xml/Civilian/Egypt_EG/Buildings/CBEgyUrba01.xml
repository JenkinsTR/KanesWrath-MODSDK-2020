	<GameObject
		id="CBEgyUrba01"
		inheritFrom="BaseCivilianStructure"
		CommandSet="GarrisonableNeutralBuildingCommandSet"
		EditorName="Egypt Urban Building 01"
        EditorSorting="STRUCTURE"
		Browser="REGION EGYPT_EG STRUCTURE">
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw"
				OkToChangeModelColor="false"
				AffectedByStealth="false"
				ExtraPublicBone="BONEGARRISON01 BONEGARRISON02 BONEGARRISON03 BONEGARRISON04 BONEGARRISON05 BONEGARRISON06 BONEGARRISON07 BONEGARRISON08 BONEGARRISON09 BONEGARRISON10 BONEGARRISON11 BONEGARRISON12">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="EBUrban01" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="RUBBLE">
					<Model
						Name="EBUrban01_D3" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="REALLYDAMAGED">
					<Model
						Name="EBUrban01_D2" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DAMAGED">
					<Model
						Name="EBUrban01_D1" />
				</ModelConditionState>
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT">
					<!-- no anim -->
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="RUBBLE"
					StateName="STATE_Rubble"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="EBUrban01_D3"
						AnimationMode="MANUAL" />
					<Script>
            Prev = CurDrawablePrevAnimationState();
            if Prev ~= null and Prev ~= "STATE_Rubble" and Prev ~= "TRANS_IntoRubble" then
              CurDrawableSetTransitionAnimState("TRANS_IntoRubble")
            end
          </Script>
        </AnimationState>
        <AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="REALLYDAMAGED"
					StateName="STATE_ReallyDamaged"
					Flags="START_FRAME_LAST">
          <Animation
						AnimationName="EBUrban01_D2"
						AnimationMode="MANUAL" />
          <Script>
            Prev = CurDrawablePrevAnimationState();
            if Prev ~= null and Prev ~= "STATE_ReallyDamaged" and Prev ~= "TRANS_IntoReallyDamaged" then
              CurDrawableSetTransitionAnimState("TRANS_IntoReallyDamaged")
            end
          </Script>
        </AnimationState>
        <AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DAMAGED"
					StateName="STATE_Damaged"
					Flags="START_FRAME_LAST">
          <Animation
						AnimationName="EBUrban01_D1"
						AnimationMode="MANUAL" />
          <Script>
            Prev = CurDrawablePrevAnimationState();
            if Prev ~= null and Prev ~= "STATE_Damaged" and Prev ~= "TRANS_IntoDamaged" then
              CurDrawableSetTransitionAnimState("TRANS_IntoDamaged")
            end
          </Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="EBUrban01_D1"
						AnimationMode="MANUAL" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_IntoDamaged">
					<Animation
						AnimationName="EBUrban01_D1"
						AnimationMode="ONCE"
						AnimationBlendTime="0" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_IntoReallyDamaged">
					<Animation
						AnimationName="EBUrban01_D2"
						AnimationMode="ONCE"
						AnimationBlendTime="0" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_IntoRubble">
					<Animation
						AnimationName="EBUrban01_D3"
						AnimationMode="ONCE"
						AnimationBlendTime="0" />
					<ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="CivBuildingDust"
						FollowBone="false" />
					<ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="CivBuildingDust02"
						FollowBone="false" />
				</AnimationState>
			</ScriptedModelDraw>
			<xi:include
				href="DATA:Includes/GarrisonFlagArmorDraw.xml"
				xpointer="xmlns(n=uri:ea.com:eala:asset) xpointer(/n:CommonArmorDraws/child::*)" />
		</Draws>
		<Behaviors>
			<FXListBehavior
				id="ModuleTag_FXList"
				xai:joinAction="Replace" xmlns:xai="uri:ea.com:eala:asset:instance">
				<DieMuxData
					DeathTypes="ALL" />
				<Event
					Index="onTransitionToDamaged"
					FX="FX_BuildDamaged_Large" />
				<Event
					Index="onTransitionToReallyDamaged"
					FX="FX_BuildDamaged_Large" />
				<Event
					Index="onTransitionToRubble"
					FX="FX_BuildRubble_Large" />
				<Event
					Index="onDeath"
					FX="FX_ScorchSmall" />
			</FXListBehavior>
		</Behaviors>
		<AI>
