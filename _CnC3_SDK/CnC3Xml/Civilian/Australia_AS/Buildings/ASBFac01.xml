				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="ASBFac01" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="RUBBLE">
					<Model
						Name="ASBFac01_D3" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
