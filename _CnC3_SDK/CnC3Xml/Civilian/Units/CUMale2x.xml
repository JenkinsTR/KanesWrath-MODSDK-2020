	<GameObject
		id="CivilianMale2"
		inheritFrom="BaseCivilian"
		SelectPortrait="Portrait_CivilianMale"
		ButtonImage="Portrait_CivilianMale"
		EditorName="CivilianMale2"
		Description="Desc:CivilianMale">
		<DisplayName
			xai:joinAction="Replace" xmlns:xai="uri:ea.com:eala:asset:instance">Name:CivilianMale</DisplayName>
		<LocomotorSet
			Locomotor="HumanLocomotor"
			Condition="NORMAL"
			Speed="17.0" />
		<LocomotorSet
			Locomotor="HumanLocomotor"
			Condition="PANIC"
			Speed="75.0" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw"
				OkToChangeModelColor="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="CUMale1_SKN" />
					<Texture
						Original="CUMale1"
						New="CUMale2" />
				</ModelConditionState>
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT"
					Flags="RESTART_ANIM_WHEN_COMPLETE">
					<Animation
						AnimationName="CUMale1_BIFA"
						AnimationMode="ONCE"
						AnimationPriority="1" />
					<Animation
						AnimationName="CUMale1_BIFB"
						AnimationMode="ONCE"
						AnimationPriority="1" />
					<Script>
						if Prev == "STATE_stunned_flailing" then
						CurDrawableSetTransitionAnimState("STATE_stunned_landing")
						end
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STUNNED_FLAILING"
					StateName="STATE_stunned_flailing">
					<Animation
						AnimationName="CUMale1_FLYA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					StateName="STATE_stunned_landing">
					<Animation
						AnimationName="CUMale1_LNDA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING">
					<Animation
						AnimationName="CUMale1_DIEA"
						AnimationMode="ONCE"
						AnimationPriority="1" />
					<Animation
						AnimationName="CUMale1_DIEB"
						AnimationMode="ONCE"
						AnimationPriority="1" />
					<Animation
						AnimationName="CUMale1_DIED"
						AnimationMode="ONCE"
						AnimationPriority="1" />
					<Animation
						AnimationName="CUMale1_DIEE"
						AnimationMode="ONCE"
						AnimationPriority="1" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING LOCOMOTOR_PANIC"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="CUMale1_RUNA"
						AnimationMode="LOOP" />
					<Animation
						AnimationName="CUMale1_RUNB"
						AnimationMode="LOOP" />
					<Animation
						AnimationName="CUMale1_RUNC"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="CUMale1_WLKA"
						AnimationMode="LOOP" />
					<Animation
