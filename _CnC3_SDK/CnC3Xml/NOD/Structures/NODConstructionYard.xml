				OkToChangeModelColor="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="NBConYard" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="UNPACKING REALLYDAMAGED">
					<Model
						Name="NBMCV_A" />
					<Texture
						Original="NBConYard"
						New="NBConYardD" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="UNPACKING DAMAGED">
					<Model
						Name="NBMCV_A" />
					<Texture
						Original="NBConYard"
						New="NBConYardD" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="UNPACKING">
					<Model
						Name="NBMCV_A" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DAMAGED">
					<Model
						Name="NBConYardD2_SN" />
					<ParticleSysBone
						BoneName="FXBone01"
						FXParticleSystemTemplate="NODConDamagedSmoke"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone02"
						FXParticleSystemTemplate="GDISiloDamagedSteam"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone02"
						FXParticleSystemTemplate="GDISiloDamagedDist2"
						FollowBone="true" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="REALLYDAMAGED">
					<Model
						Name="NBConYardD2_SN" />
					<ParticleSysBone
						BoneName="FXBone01"
						FXParticleSystemTemplate="NODConDamagedSmoke"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone01"
						FXParticleSystemTemplate="NODRefineryDamagedFire"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone01"
						FXParticleSystemTemplate="NODRefineryDamagedDist"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone02"
						FXParticleSystemTemplate="NODConDamagedDist"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone02"
						FXParticleSystemTemplate="NODConDamagedVent"
						FollowBone="true" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING">
					<Model
						Name="NBConYardD3_SN" />
				</ModelConditionState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="UNPACKING">
					<Animation
						AnimationName="NBMCV_A"
						AnimationMode="ONCE"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="0.8" />
				</AnimationState>
				<!-- DAMAGED ANIMATION -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DAMAGED"
					StateName="STATE_Damaged"
					Flags="START_FRAME_FIRST">
					<Animation
						AnimationName="NBConYardD2_AN"
						AnimationMode="MANUAL" />
				</AnimationState>
				<!-- REALLY DAMAGED ANIMATION -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="REALLYDAMAGED"
					StateName="STATE_ReallyDamaged"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="NBConYardD2_AN"
						AnimationMode="MANUAL" />
					<Script>
					Prev = CurDrawablePrevAnimationState();
					if Prev ~= "STATE_ReallyDamaged" and Prev ~= "TRANS_DamagedToReallyDamaged" then
						CurDrawableSetTransitionAnimState("TRANS_DamagedToReallyDamaged")
					end
				</Script>
				</AnimationState>
				<!-- DYING ANIMATION -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING">
					<Animation
						AnimationName="NBConYardD3_AN"
						AnimationMode="ONCE" />
					<ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="Des_Large"
						FollowBone="false" />
				</AnimationState>
				<!-- Transitions -->
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_DamagedToReallyDamaged">
					<Animation
						AnimationName="NBConYardD2_AN"
						AnimationMode="ONCE" />
				</AnimationState>
			</ScriptedModelDraw>
			<!-- DRAW GAG ANIMATION -->
			<ScriptedModelDraw
				id="ModuleTag_Draw_Gag"
				OkToChangeModelColor="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="NBConArm_SKN" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STRUCTURE_UNPACKING">
					<Model
						Name="NBEmpty_ABLD" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="UNPACKING">
					<Model
						Name="NBEmpty_ABLD" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DAMAGED">
					<Model
						Name="NBConArm_SKN" />
					<Texture
						Original="NBConYard"
						New="NBConYardD" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="REALLYDAMAGED">
					<Model
						Name="NBConArm_SKN" />
					<Texture
						Original="NBConYard"
						New="NBConYardD" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING">
					<Model
						Name="NBEmpty_ABLD" />
				</ModelConditionState>	
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT"
					StateName="STATE_Closed">
					<Animation
						AnimationName="NBConArm_IDLA"
						AnimationMode="LOOP" />
				</AnimationState>
				<!-- BUILDUP ANIMATION -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STRUCTURE_UNPACKING">
					<Animation
						AnimationName="NBEmpty_ABLD"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="UNPACKING">
					<Animation
						AnimationName="NBEmpty_ABLD"
						AnimationMode="ONCE"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="0.8" />
				</AnimationState>
				<!--
				<AnimationState
							ParseCondStateType="PARSE_DEFAULT"
							StateName="STATE_Closed"
							Flags="START_FRAME_FIRST">
					<Animation
								AnimationName="NBConArm_IDLA"
								AnimationMode="MANUAL" />
				</AnimationState>
				<AnimationState
							ParseCondStateType="PARSE_NORMAL"
							ConditionsYes="ACTIVELY_CONSTRUCTING">
					<Animation
								AnimationName="NBConArm_IDLA"
								AnimationMode="ONCE" />
				</AnimationState>
-->
			</ScriptedModelDraw>
		</Draws>
