						Bone="B_BACKRIGHT" />
					<FXEvent
						Frame="15"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_FRONTLEFT" />
					<FXEvent
						Frame="29"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_FRONTRIGHT" />
					<FXEvent
						Frame="29"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_BACKLEFT" />
					<FXEvent
						Frame="45"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_BACKRIGHT" />
					<FXEvent
						Frame="45"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_FRONTLEFT" />
					<FXEvent
						Frame="60"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_FRONTRIGHT" />
					<FXEvent
						Frame="60"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_BACKLEFT" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="TURN_RIGHT MOVING"
					StateName="STATE_Run"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="NBMCV_TRNR"
						AnimationMode="LOOP"
						Distance="65" />
					<FXEvent
						Frame="14"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_FRONTRIGHT" />
					<FXEvent
						Frame="15"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_BACKLEFT" />
					<FXEvent
						Frame="29"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_BACKRIGHT" />
					<FXEvent
						Frame="29"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_FRONTLEFT" />
					<FXEvent
						Frame="45"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_FRONTRIGHT" />
					<FXEvent
						Frame="45"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_BACKLEFT" />
					<FXEvent
						Frame="60"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_BACKRIGHT" />
					<FXEvent
						Frame="60"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_FRONTEFT" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING"
					StateName="STATE_Run"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="NBMCV_WLKA"
						AnimationMode="LOOP"
						Distance="65" />
					<FXEvent
						Frame="36"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_FRONTLEFT" />
					<FXEvent
						Frame="70"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_BACKLEFT" />
					<FXEvent
						Frame="36"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_BACKRIGHT" />
					<FXEvent
						Frame="70"
						FireWhenSkipped="true"
						Effect="FX_NODMCVFootprint"
						Bone="B_FRONTRIGHT" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_built" then
						CurDrawableSetTransitionAnimState("TRANS_built")
						end
					</Script>						
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_built">
					<Animation
						AnimationName="NBMCV_BLDA"
						AnimationMode="ONCE"
						AnimationBlendTime="0"/>
				</AnimationState>				
