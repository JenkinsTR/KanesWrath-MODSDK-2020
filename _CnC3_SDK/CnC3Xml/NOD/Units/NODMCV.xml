					StateName="STATE_bored">
					<Animation
						AnimationName="NBMCV_IDLA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="COMING_OUT_OF_FACTORY"
					Flags="START_FRAME_FIRST"
