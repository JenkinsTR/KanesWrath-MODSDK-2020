					StateName="STATE_Run"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="NBMCV_TRNL"
						AnimationMode="LOOP"
						Distance="65" />
					<FXEvent
						Frame="14"
						FireWhenSkipped="true"
