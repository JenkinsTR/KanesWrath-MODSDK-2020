					<Model
						Name="NBMCV_A" />
					<Texture
						Original="NBConYard"
						New="NBConYardD" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="UNPACKING DAMAGED">
					<Model
						Name="NBMCV_A" />
					<Texture
						Original="NBConYard"
						New="NBConYardD" />
				</ModelConditionState>
				<ModelConditionState
