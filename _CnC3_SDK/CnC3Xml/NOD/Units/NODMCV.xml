		Side="NOD"
		EditorSorting="UNIT"
		TransportSlotCount="1"
		BuildCost="3500"
		BuildTime="35"
		BuildFadeInOnCreateTimeSeconds="0.25s"
		CommandSet="NODMCVCommandSet"
		CommandPoints="100" 
		KindOf="PRELOAD SELECTABLE CAN_CAST_REFLECTIONS SCORE VEHICLE FS_FACTORY EXPANSION_UNIT CAN_BE_FAVORITE_UNIT MCV"
		RadarPriority="UNIT"
		PlacementViewAngle="315d"
		ProductionQueueType="VEHICLE"
		UnitCategory="VEHICLE"
		VoicePriority="36"
		EditorName="NODMCV"
		Description="Desc:NODMCV"
		TypeDescription="Type:NODMCV">
