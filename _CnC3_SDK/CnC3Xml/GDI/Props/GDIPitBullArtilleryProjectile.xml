<?xml version="1.0" encoding="us-ascii"?>
<!-- The default rocket pod projectile -->
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Tags></Tags>
	<Includes>
		<Include
			type="all"
			source="ART:NBRDefMis.w3x" />
	</Includes>
	<GameObject
		id="GDIPitBullArtilleryProjectile"
		Side="GDI"
		EditorSorting="SYSTEM"
		KindOf="PROJECTILE INERT DEFLECT_BY_SPECIAL_POWER NO_COLLIDE HIDE_IF_FOGGED"
		EditorName="GDIPitBullArtilleryProjectile">
		<ArmorSet
			Armor="NoArmor" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="NBRDefMis" />
					<ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="orcamissiletrail"
						FollowBone="true" />
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<DestroyDie
				id="ModuleTag_Die">
				<DieMuxData
					DeathTypes="ALL" />
			</DestroyDie>
			<FXListBehavior
				id="ModuleTag_FXList">
				<DieMuxData
					DeathTypes="ALL" />
				<Event
					Index="onDeath"
					FX="FX_MediumXplosion" />
			</FXListBehavior>
			<BezierProjectile
				id="ModuleTag_BezierProjectileBehavior"
				FirstHeightMin="170"
				FirstHeightMax="200"
				SecondHeightMin="170"
				SecondHeightMax="200"
				FirstPercentIndent="20"
				SecondPercentIndent="90"
				OrientToFlightPath="true"
				DetonateCallsKill="true" 
			/>
		</Behaviors>
		<Body>
			<ActiveBody
				id="ModuleTag_ActiveBody"
				MaxHealth="100.0" />
		</Body>
		<Geometry
			IsSmall="true">
			<Shape
				Type="SPHERE"
				MajorRadius="0.8"></Shape>
		</Geometry>
		<VisionInfo
			VisionRange="0" />
	</GameObject>
</AssetDeclaration>