	<GameObject
		id="GDIEngineer"
		inheritFrom="BaseInfantry"
		SelectPortrait="Portrait_GDIEngineer"
		ButtonImage="Portrait_GDIEngineer"
		Side="GDI"
		EditorSorting="UNIT"
		TransportSlotCount="1"
		BuildCost="500"
		BuildTime="5"
		CommandSet="GDIEngineerCommandSet"
		IsTrainable="true"
		KindOf="PRELOAD SELECTABLE ATTACK_NEEDS_LINE_OF_SIGHT CAN_CAST_REFLECTIONS INFANTRY SCORE CAN_CAPTURE PATH_THROUGH_INFANTRY SKIRMISH_AI_DONT_GARRISON ENGINEER CAN_CAPTURE CAN_BE_FAVORITE_UNIT"
		RadarPriority="UNIT"
		BuildCompletion="APPEARS_AT_RALLY_POINT"
		ThreatLevel="10"
		ProductionQueueType="INFANTRY"
		UnitCategory="INFANTRY"
		VoicePriority="68"
		EditorName="GDIEngineer"
		Description="Desc:GDIEngineer"
		TypeDescription="Type:GDIEngineer"
		UnitTypeIcon="EngineerIcon">
		<DisplayName
			xai:joinAction="Replace" xmlns:xai="uri:ea.com:eala:asset:instance">Name:GDIEngineer</DisplayName>
		<!--<Prerequisites
			Object="GDIBarracks" />-->
		<ArmorSet
			Armor="GDIEngineerArmor"
			DamageFX="InfantryDamageFX" />
		<LocomotorSet
			Locomotor="HumanLocomotor"
			Condition="NORMAL"
			Speed="36" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw"
				OkToChangeModelColor="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="GUEngineer_SKN" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="FORMATION_PREVIEW">
					<Model
						Name="FXEngineer_FP" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="INSIDE_GARRISON">
					<Model
						Name="" />
				</ModelConditionState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="FORMATION_PREVIEW">
					<!-- no anim -->
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT"
					Flags="RESTART_ANIM_WHEN_COMPLETE"
					AllowRepeatInRandomPick="true"
					StateName="STATE_Bored">
					<Animation
						AnimationName="GUEngineer_BIDA"
						AnimationMode="ONCE"
						AnimationPriority="10" />
					<Animation
						AnimationName="GUEngineer_BIFA"
						AnimationMode="ONCE"
						AnimationPriority="1" />
					<Animation
						AnimationName="GUEngineer_BIFB"
						AnimationMode="ONCE"
						AnimationPriority="5" />
					<Animation
						AnimationName="GUEngineer_BIFC"
						AnimationMode="ONCE"
						AnimationPriority="5" />
					<Animation
						AnimationName="GUEngineer_BIFD"
						AnimationMode="ONCE"
						AnimationPriority="1" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_Suppressed" then
						CurDrawableSetTransitionAnimState("TRANS_fromSuppressed")
						end
						if Prev == "STATE_Selected" then
						CurDrawableSetTransitionAnimState("TRANS_attentionToBored")
						end
						if Prev == "STATE_stunned_flailing" then
						CurDrawableSetTransitionAnimState("STATE_stunned_landing")
						end
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STUNNED_FLAILING"
					StateName="STATE_stunned_flailing">
					<Animation
						AnimationName="GUEngineer_FLYA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					StateName="STATE_stunned_landing">
					<Animation
						AnimationName="GUEngineer_LNDA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="FRONTCRUSHED">
					<Animation
						AnimationName="GUEngineer_CDTA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="BACKCRUSHED">
					<Animation
						AnimationName="GUEngineer_CDTB"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="COVER DYING"
					StateName="STATE_Cover">
					<Animation
						AnimationName="GUEngineer_KDIA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING">
					<Animation
						AnimNickName="DTA"
						AnimationName="GUEngineer_DIEA"
						AnimationMode="ONCE" />
				</AnimationState>
				<!--Supressed-->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SUPPRESSED MOVING"
					StateName="STATE_Suppressed"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="GUEngineer_SMVA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SUPPRESSED"
					Flags="RESTART_ANIM_WHEN_COMPLETE"
					StateName="STATE_Suppressed">
					<Animation
						AnimationName="GUEngineer_SIDA"
						AnimationMode="ONCE" />
					<Animation
						AnimationName="GUEngineer_SIFA"
						AnimationMode="ONCE" />
					<Animation
						AnimationName="GUEngineer_SIFB"
						AnimationMode="ONCE" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev ~= "STATE_Suppressed" and Prev ~= "TRANS_toSuppressed" then
						CurDrawableSetTransitionAnimState("TRANS_toSuppressed")
						end
					</Script>
				</AnimationState>
				<!--Cover-->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="COVER MOVING"
					Flags="RANDOMSTART"
					StateName="STATE_Cover">
					<Animation
						AnimationName="GUEngineer_CRNA"
						AnimationMode="LOOP"
						Distance="75" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SELECTED COVER"
					StateName="STATE_Cover">
					<Animation
						AnimationName="GUEngineer_KAIA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="COVER"
					Flags="RESTART_ANIM_WHEN_COMPLETE"
					StateName="STATE_Cover">
					<Animation
						AnimationName="GUEngineer_KBIA"
						AnimationMode="ONCE"
						AnimationPriority="15"
						AnimationBlendTime="5"
						AnimationMustCompleteBlend="true" />
					<Animation
						AnimationName="GUEngineer_KBIA"
						AnimationMode="ONCE"
						AnimationPriority="15"
						AnimationBlendTime="5"
						AnimationMustCompleteBlend="true" />
					<Animation
						AnimationName="GUEngineer_KBFA"
						AnimationMode="ONCE"
						AnimationPriority="7"
						AnimationBlendTime="5"
						AnimationMustCompleteBlend="true" />
					<Animation
						AnimationName="GUEngineer_KBFB"
						AnimationMode="ONCE"
						AnimationPriority="5"
						AnimationBlendTime="5"
						AnimationMustCompleteBlend="true" />
					<Animation
						AnimationName="GUEngineer_KBFC"
						AnimationMode="ONCE"
						AnimationPriority="7"
						AnimationBlendTime="5"
						AnimationMustCompleteBlend="true" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="GUEngineer_RUNA"
						AnimationMode="LOOP"
						AnimationSpeedFactorMin="1.0"
						AnimationSpeedFactorMax="1.0" />
				</AnimationState>
				<!-- EMOTIONS -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="EMOTION_CELEBRATING"
					Flags="RESTART_ANIM_WHEN_COMPLETE">
					<Animation
						AnimationName="GUEngineer_CHRA"
						AnimationMode="ONCE" />
					<Animation
						AnimationName="GUEngineer_CHRB"
						AnimationMode="ONCE" />
				</AnimationState>
				<!--Selected-->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SELECTED"
					Flags="RESTART_ANIM_WHEN_COMPLETE"
					StateName="STATE_Selected">
					<Animation
						AnimationName="GUEngineer_AIDA"
						AnimationMode="ONCE"
						AnimationPriority="15"
						AnimationBlendTime="5"
						AnimationMustCompleteBlend="true" />
					<Animation
						AnimationName="GUEngineer_AIFA"
						AnimationMode="ONCE"
						AnimationPriority="5"
						AnimationBlendTime="5"
						AnimationMustCompleteBlend="true" />
					<Animation
						AnimationName="GUEngineer_AIFB"
						AnimationMode="ONCE"
						AnimationPriority="5"
						AnimationBlendTime="5"
						AnimationMustCompleteBlend="true" />
					<Animation
						AnimationName="GUEngineer_AIFC"
						AnimationMode="ONCE"
						AnimationPriority="2"
						AnimationBlendTime="5"
						AnimationMustCompleteBlend="true" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_Suppressed" then
						CurDrawableSetTransitionAnimState("TRANS_fromSuppressedB")
						end
						if Prev == "STATE_Bored" then
						CurDrawableSetTransitionAnimState("TRANS_boredToAttention")
						end
					</Script>
				</AnimationState>
				<!-- TRANSITIONS -->
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_boredToAttention">
					<Animation
						AnimationName="GUEngineer_BATA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_attentionToBored">
					<Animation
						AnimationName="GUEngineer_ABTA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_toSuppressed">
					<Animation
						AnimationName="GUEngineer_SDVA"
						AnimationMode="ONCE"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
				</AnimationState>
				<!--
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_fromSuppressed">
					<Animation
						AnimationName="GUEngineer_SGTB"
						AnimationMode="ONCE"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
				</AnimationState>
        -->
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_fromSuppressedB">
					<Animation
						AnimationName="GUEngineer_SGTA"
						AnimationMode="ONCE"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
				</AnimationState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<Physics
				id="ModuleTag_07" />
			<ProductionUpdate
				id="ModuleTag_08" />
			<SubObjectsUpgrade
				id="ModuleTag_HideAll"
				HideSubObjects="UGRAILGUN UGARMORCERAMIC UGGRENADE UGSCANNER">
				<TriggeredBy>Upgrade_GDIFaction</TriggeredBy>
			</SubObjectsUpgrade>
			<StealthDetectorUpdate
				id="ModuleTag_StealthDetect"
				DetectionRange="100" />
			<EmotionTrackerUpdate
				id="Module_ETU">
				<TauntAndPointExcluded
					Rule="NONE" />
				<AddEmotion>CheerIdle_Base</AddEmotion>
				<AddEmotion>CheerBusy_Base</AddEmotion>
			</EmotionTrackerUpdate>
			<SlowDeath
				id="ModuleTag_Death"
				SinkDelay="6s"
				SinkRate="2.4"
				DestructionDelay="7.5s">
				<DieMuxData
					DeathTypes="ALL" />
				<Sound
					Type="INITIAL"
					List="GDI_Engineer_VoiceDie" />
			</SlowDeath>
			<SuppressionUpdate
				id="ModuleTag_SuppressionUpdate"
				UpdateDelay="1s"
				Suppressability="500"
				SuppressionDuration="5s"
				AttributeModifierSuppressed="Modifier_Test_Suppression"
				AttributeModifierForceMove="Modifier_Test_Suppression_ForceMove" />
			<FindCoverBehavior
				id="ModuleTag_FCB"
				CoverScanRange="10.0"
				AttributeModifierInCover="Modifier_Cover" />
			<AISpecialPowerUpdate
				id="ModuleTag_GDIEngineerCaptureBuildingAI"
				CommandButtonName="Command_CaptureBuilding"
				SpecialPowerAIType="SPECIAL_POWER_CAPTURE_BUILDING" />
			<xi:include
				href="DATA:Includes/GDICallForTransport.xml" />
			<xi:include
				href="DATA:Includes/CaptureBehavior.xml"
				xpointer="xmlns(n=uri:ea.com:eala:asset) xpointer(/n:Behaviors/child::*)" />
		</Behaviors>
		<AI>
			<AIUpdate
				id="ModuleTag_06"
				AutoAcquireEnemiesWhenIdle="YES" />
		</AI>
		<Body>
			<ActiveBody
				id="ModuleTag_03"
				MaxHealth="75.0" />
		</Body>
		<Geometry
			IsSmall="true">
			<Shape
				Type="CYLINDER"
				MajorRadius="7.0"
				MinorRadius="7.0"
				Height="13.0">
				<Offset
					x="0"
					y="0"
					z="0" />
			</Shape>
		</Geometry>
		<AudioArrayVoice>
			<AudioEntry
				Sound="GDI_Engineer_VoiceCreate"
				AudioType="voiceCreated" />
			<AudioEntry
				Sound="GDI_Engineer_VoiceGarrison"
				AudioType="voiceGarrison" />
			<AudioEntry
				Sound="GDI_Engineer_VoiceMove"
				AudioType="voiceMove" />
			<AudioEntry
				Sound="GDI_Engineer_VoiceRepair"
				AudioType="voiceRepair" />
			<AudioEntry
				Sound="GDI_Engineer_VoiceRetreat"
				AudioType="voiceRetreatToCastle" />
			<AudioEntry
				Sound="GDI_Engineer_VoiceSelectMS"
				AudioType="voiceSelect" />
			<!-- Voice for being ordered to enter and repair or capture various things. The names are defined in the 
			     various EngineerContain modules on the target objects -->
			<NamedEntry
				Sound="GDI_Engineer_VoiceRepair"
				Name="VoiceEngineerRepair" />
			<NamedEntry
				Sound="GDI_Engineer_VoiceCaptureBuilding"
				Name="VoiceCaptureBuilding" />
			<NamedEntry
				Sound="GDI_Engineer_VoiceCaptureHusk"
				Name="VoiceCaptureHusk" />
		</AudioArrayVoice>
