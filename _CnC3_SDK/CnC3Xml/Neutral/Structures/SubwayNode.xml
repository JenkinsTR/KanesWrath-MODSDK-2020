			source="DATA:BaseObjects/BaseTechStructure.xml" />
	</Includes>
	<GameObject
		id="SubwayNode"
		inheritFrom="BaseTechStructure"
		SelectPortrait="Portrait_TechBuildingSubwayNode"
		ButtonImage="Portrait_TechBuildingSubwayNode"
		Side="Neutral"
		EditorSorting="STRUCTURE"
		BuildTime="15"
		CommandSet="SubwayCommandSet"
		KindOf="STRUCTURE SELECTABLE IMMOBILE CAN_CAST_REFLECTIONS FS_TECHNOLOGY NOT_AUTOACQUIRABLE IGNORE_FOR_VICTORY NOT_SELLABLE AUTO_RALLYPOINT"
		RadarPriority="STRUCTURE"
		EditorName="SubwayNode"
		Description="Desc:TechBuildingSubwayNode">
		<DisplayName
			xai:joinAction="Replace" xmlns:xai="uri:ea.com:eala:asset:instance">Name:TechBuildingSubwayNode</DisplayName>
		<ArmorSet
			Armor="StructureArmor" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw"
				OkToChangeModelColor="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="CBSubEnt" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="RUBBLE">
					<Model
						Name="CBSubEnt_D3" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="REALLYDAMAGED">
					<Model
						Name="CBSubEnt_D2" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DAMAGED">
					<Model
						Name="CBSubEnt_D1" />
				</ModelConditionState>
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT">
					<!-- no anim -->
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="RUBBLE"
					StateName="STATE_Rubble"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="CBSubEnt_D3"
						AnimationMode="MANUAL" />
					<Script>
						Prev = CurDrawablePrevAnimationState();
						if Prev ~= "STATE_Rubble" and Prev ~= "TRANS_IntoRubble" then
						CurDrawableSetTransitionAnimState("TRANS_IntoRubble")
						end
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="REALLYDAMAGED"
					StateName="STATE_ReallyDamaged"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="CBSubEnt_D2"
						AnimationMode="MANUAL" />
					<Script>
						Prev = CurDrawablePrevAnimationState();
						if Prev ~= "STATE_ReallyDamaged" and Prev ~= "TRANS_IntoReallyDamaged" then
						CurDrawableSetTransitionAnimState("TRANS_IntoReallyDamaged")
						end
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_IntoReallyDamaged">
					<Animation
						AnimationName="CBSubEnt_D2"
						AnimationMode="ONCE"
						AnimationBlendTime="0" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_IntoRubble">
					<Animation
						AnimationName="CBSubEnt_D3"
						AnimationMode="ONCE"
						AnimationBlendTime="0" />
				</AnimationState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<SlowDeath
				id="ModuleTag_Death"
				SinkDelay="4s"
				SinkRate="3.0"
				DestructionDelay="8.0s">
				<Sound Type="INITIAL" List="HumanFaction_MediumBuilding_DieMS" />
				<DieMuxData
					DeathTypes="ALL" />
			</SlowDeath>
			<TunnelContain
				id="ModuleTag_TunnelContain"
				ObjectStatusOfContained="UNSELECTABLE ENCLOSED"
				ContainMax="3"
				AllowEnemiesInside="false"
				AllowAlliesInside="false"
				AllowNeutralInside="false"
				AllowOwnPlayerInsideOverride="true"
				ShowPips="false"
				DeleteRemoved="true"
				TunnelMasterObject="SubwayHub">
				<PassengerFilter
					Rule="ANY"
					Include="INFANTRY BANNER CAVALRY"
					Exclude="MONSTER SUMMONED COMBO_HORDE" />
				<DieMuxData
					DeathTypes="ALL" />
				<PassengerData
					BonePrefix="ARROW_">
					<Filter
						Rule="ANY"
						Include="INFANTRY" />
				</PassengerData>
				<EntryOffset
					x="0"
					y="0"
					z="0" />
				<EntryPosition
					x="30"
					y="0"
					z="0" />
				<ExitOffset
					x="80"
					y="0"
					z="0" />
			</TunnelContain>
			<xi:include
				href="DATA:Includes/GenericBuildingRepair.xml" />
		</Behaviors>
		<AI>
			<AIUpdate
				id="ModuleTag_AI"
				AutoAcquireEnemiesWhenIdle="NO"
				AILuaEventsList="BuildingPowerFunctions">
			</AIUpdate>
		</AI>
		<Body>
			<ActiveBody
				id="ModuleTag_Body"
