<?xml version="1.0" encoding="utf-8"?>
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xi="http://www.w3.org/2001/XInclude">
	<Tags></Tags>
	<Includes>
		<Include
			type="instance"
			source="DATA:BaseObjects/BaseTechStructure.xml" />
		<Include
			type="all"
			source="ART:GBIonCan.w3x" />
		<Include
			type="all"
			source="ART:GBRadar.w3x" />
	</Includes>
	<GameObject
		id="IonStormControlCenter"
		inheritFrom="BaseTechStructure"
		SelectPortrait="Button_GDIBarracks"
		ButtonImage="Button_GDIBarracks"
		Side="Neutral"
		BuildTime="15"
		EditorSorting="STRUCTURE"
		CommandSet="IonStormControlCenterCommandSet"
		KindOf="STRUCTURE SELECTABLE IMMOBILE CAN_CAST_REFLECTIONS FS_TECHNOLOGY NOT_AUTOACQUIRABLE IGNORE_FOR_VICTORY NOT_SELLABLE NEUTRAL_TECH"
		RadarPriority="STRUCTURE"
		EditorName="IonStormControlCenter"
		Description="Desc:TechBuildingIonStormControlCenter">
		<DisplayName
			xai:joinAction="Replace" xmlns:xai="uri:ea.com:eala:asset:instance">Name:TechBuildingIonStormControlCenter</DisplayName>
		<ArmorSet
			Armor="StructureArmor" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="GBIonCan" />
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<CreateObjectDie
				id="ModuleTag_CreateObjectDie"
				CreationList="OCL_IonStormControlCenter_Destroyed">
				<DieMuxData
					DeathTypes="ALL" />
			</CreateObjectDie>
			<DestroyDie
				id="ModuleTag_Die">
				<DieMuxData
					DeathTypes="ALL" />
			</DestroyDie>
			<xi:include
				href="DATA:Includes/GenericEngineerContain.xml" />
			<xi:include
				href="DATA:Includes/GenericBuildingRepair.xml" />
		</Behaviors>
		<Body>
			<ActiveBody
				id="ModuleTag_Body"
				MaxHealth="3000.0"
				InitialHealth="3000.0" />
		</Body>
		<Geometry
			IsSmall="false">
			<Shape
				Type="BOX"
				MajorRadius="40.0"
				MinorRadius="40.0"
				Height="40.0"></Shape>
		</Geometry>
		<ShadowInfo
			Type="VOLUME" />
		<VisionInfo
			VisionRange="100"
			ShroudClearingRange="300" />
		<ProjectedBuildabilityInfo
			Radius="350.0"
			BuildPlacementTypes="MAIN_STRUCTURE OTHER_STRUCTURE" />
	</GameObject>
	<GameObject
		id="IonStormControlCenter_Destroyed"
		SelectPortrait="Button_GDIBarracks"
		ButtonImage="Button_GDIBarracks"
		Side="Neutral"
		EditorSorting="STRUCTURE"
		KindOf="STRUCTURE SELECTABLE IMMOBILE CAN_CAST_REFLECTIONS FS_TECHNOLOGY IGNORE_FOR_VICTORY NOT_AUTOACQUIRABLE NOT_SELLABLE UNATTACKABLE"
		RadarPriority="STRUCTURE"
		EditorName="IonStormControlCenter_Destroyed">
		<DisplayName>IonStormControlCenter_Destroyed</DisplayName>
		<ArmorSet
			Armor="StructureArmor" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="GBRadar" />
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<EngineerContain
				id="ModuleTag_EngineerContain"
				NameOfVoiceToUseForFriendlyEnter="VoiceEngineerRepair"
				NameOfVoiceToUseForHostileEnter="VoiceEngineerRepair"
				EvaEventForReplace="BuildingRepaired"
				EvaEventForCaptureAndReplace="EnemyBuildingCaptured"
				FXForRepair="FX_Building_Repaired"
				FXForCapture="FX_Building_Captured"
				FXForCaptureAndReplace="FX_Building_Captured"
				ReplaceWith="OCL_IonStormControlCenter_Rebuilt">
				<CanEnterFilter
					Rule="NONE">
					<IncludeThing>GDIEngineer</IncludeThing>
					<IncludeThing>NODSaboteur</IncludeThing>
					<IncludeThing>AlienAssimilator</IncludeThing>
				</CanEnterFilter>
			</EngineerContain>
		</Behaviors>
		<Body>
			<ImmortalBody
				id="ModuleTag_Body"
				MaxHealth="1.0"
				InitialHealth="1.0" />
		</Body>
		<Geometry
			IsSmall="false">
			<Shape
				Type="BOX"
				MajorRadius="40.0"
				MinorRadius="40.0"
				Height="40.0"></Shape>
		</Geometry>
		<ShadowInfo
			Type="VOLUME" />
		<VisionInfo
			VisionRange="100"
			ShroudClearingRange="300" />
	</GameObject>
</AssetDeclaration>