					<Animation
						AnimationName="AURazDro_ABTB"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_RunToCover">
					<Animation
						AnimationName="AURazDro_KATB"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
