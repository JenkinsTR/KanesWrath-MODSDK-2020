		<DisplayUpgrade id="ModuleTag_DisplayCompositeArmorUpgrade">Upgrade_GDICompositeArmor</DisplayUpgrade>
		<DisplayUpgrade id="ModuleTag_DisplayEMPGrenadesUpgrade">Upgrade_EMPGrenades</DisplayUpgrade>
		<ArmorSet
