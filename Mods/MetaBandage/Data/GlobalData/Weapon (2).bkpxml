<!--Added new subfaction units to Damage Filter-->
	<WeaponTemplate
		id="AlienBuzzerWeapon"
		Name="AlienBuzzerWeapon"
		AttackRange="30.0"
		LeechRangeWeapon="true"
		AcceptableAimDelta="360d"
		FireSound="AudioEvent:ALI_Buzzers_WeaponFire"
		CanFireWhileMoving="true"
		RadiusDamageAffects="ALLIES ENEMIES NEUTRALS"
		AntiMask="ANTI_GROUND ANTI_STRUCTURE" >

		<!-- A small amount of damage at a high frequency. -->
		<FiringDuration
			MinSeconds="0.1s"
			MaxSeconds="0.1s" />
		<Nuggets>
			<DamageNugget
				Damage="100.0"
				Radius="5.0"
				DamageType="SNIPER"
				DamageFXType="ALIEN_BUZZER">
				<DamageScalarDetails Scalar= "1%" >
					<Filter
						Rule="ANY"
						Relationship="ALLIES">
						<IncludeThing>AlienBuzzers</IncludeThing>
						<IncludeThing>AlienBuzzers_Slaved</IncludeThing>
						<IncludeThing>Reaper17Buzzers</IncludeThing>
						<IncludeThing>Traveler59Buzzers</IncludeThing>
					</Filter>
				</DamageScalarDetails>
			</DamageNugget>
		</Nuggets>
	</WeaponTemplate>
	
