				id="ModuleTag_MetaGameStratButtonInfo"
				MetaGameDescription="Desc:DisruptionGenerator"
				MetaGameTypeDescription="Type:MetaNODTempleofNod"
				>
				<!-- you can list as many powers as you want here -->
				<Powers
