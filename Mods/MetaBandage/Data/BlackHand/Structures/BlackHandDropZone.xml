						AnimationName="NBDrpZnD3_AN"
						AnimationMode="ONCE" />
					<ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="Des_Large"
						FollowBone="false" />
				</AnimationState>
				<!-- Transitions -->
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_DamagedToReallyDamaged">
					<Animation
						AnimationName="NBDrpZnD2_AN"
						AnimationMode="ONCE" />
				</AnimationState>
