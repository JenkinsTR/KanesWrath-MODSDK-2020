		<ShadowInfo
			Type="VOLUME" />
		<VisionInfo
			VisionRange="150"
			ShroudClearingRange="320" />
		<CrusherInfo
			CrusherLevel="1"
			CrushableLevel="3" />
	</GameObject>
</AssetDeclaration>