                        AnimationName="GUMisslMn_KAKB"
                        AnimationMode="LOOP" />
                </AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="COVER FIRING_OR_PREATTACK_A"
					StateName="STATE_Cover"
					FrameForPristineBonePositions="1">
					<Animation
						AnimationName="GUMisslMn_KAKA"
						AnimationMode="ONCE"
						UseWeaponTiming="true" />
				</AnimationState>
        <AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="COVER ATTACKING">
          <Animation
						AnimationName="GUMisslMn_KAKA"
						AnimationMode="MANUAL" />
        </AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="COVER SELECTED"
					Flags="RESTART_ANIM_WHEN_COMPLETE"
					StateName="STATE_CoverAttention">
					<Animation
						AnimationName="GUMisslMn_KAIA"
						AnimationMode="ONCE"
						AnimationPriority="80"
						AnimationBlendTime="10" />
					<Animation
						AnimationName="GUMisslMn_KAIB"
						AnimationMode="ONCE"
						AnimationPriority="15"
						AnimationBlendTime="5" />
					<Animation
						AnimationName="GUMisslMn_KAFA"
						AnimationMode="ONCE"
						AnimationPriority="30"
						AnimationBlendTime="5" />
					<Animation
						AnimationName="GUMisslMn_KAFB"
						AnimationMode="ONCE"
						AnimationPriority="30"
						AnimationBlendTime="5" />
					<Animation
						AnimationName="GUMisslMn_KAFC"
						AnimationMode="ONCE"
					AnimationPriority="30"
					AnimationBlendTime="5" />
                    <Script>
                        Prev = CurDrawablePrevAnimationState()
                        if Prev == "STATE_Cover" then
                        CurDrawableSetTransitionAnimState("TRANS_CoverToCoverAttention")
                        end
                    </Script>
                </AnimationState>
                <AnimationState
						ParseCondStateType="PARSE_NORMAL"
						ConditionsYes="COVER"
						AllowRepeatInRandomPick="true"
						Flags="RESTART_ANIM_WHEN_COMPLETE"
						StateName="STATE_Cover">
                    <Animation
						AnimationName="GUMisslMn_KBIA"
						AnimationMode="ONCE"
						AnimationPriority="15" 
					    AnimationBlendTime="10"/>
                    <Animation
						AnimationName="GUMisslMn_KBIB"
						AnimationMode="ONCE"
						AnimationPriority="15" 
					    AnimationBlendTime="10"/>
                    <Animation
						AnimationName="GUMisslMn_KBFA"
						AnimationMode="ONCE"
						AnimationPriority="2" 
					    AnimationBlendTime="10"/>
                    <Animation
						AnimationName="GUMisslMn_KBFB"
						AnimationMode="ONCE"
						AnimationPriority="1" 
					    AnimationBlendTime="10"/>
                    <Animation
						AnimationName="GUMisslMn_KBFC"
						AnimationMode="ONCE"
						AnimationPriority="1" />
                    <Script>
                        Prev = CurDrawablePrevAnimationState()
                        if Prev == "STATE_CoverAttention" then
                        CurDrawableSetTransitionAnimState("TRANS_CoverAttentionToCover")
                        end
                    </Script>
                </AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING USING_WEAPON_A"
					StateName="STATE_Run"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="GUMisslMn_RUNC"
						AnimationMode="LOOP"
						Distance="75" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING"
					StateName="STATE_Run"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="GUMisslMn_RUNA"
						AnimationMode="LOOP"
						AnimationBlendTime="10"
						Distance="75" />
					<Animation
						AnimationName="GUMisslMn_RUNB"
						AnimationMode="LOOP"
						AnimationBlendTime="10"
						Distance="75" />
					<Animation
						AnimationName="GUMisslMn_RUNC"
						AnimationMode="LOOP"
						AnimationBlendTime="10"
						Distance="75" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
