					Flags="RANDOMSTART">
					<Animation
						AnimationName="GUMisslMn_SMVA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SUPPRESSED USING_WEAPON_A"
					StateName="STATE_Suppressed">
