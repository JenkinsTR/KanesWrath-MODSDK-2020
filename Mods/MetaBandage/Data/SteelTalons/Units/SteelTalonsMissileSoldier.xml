					<Animation
						AnimationName="GUMisslMn_SIDA"
						AnimationMode="ONCE" />
					<Animation
						AnimationName="GUMisslMn_SIFA"
						AnimationMode="ONCE" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
