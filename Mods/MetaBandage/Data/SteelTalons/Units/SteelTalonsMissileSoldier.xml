							StateName="TRANS_CoverAttentionToCover">
                    <Animation
								  AnimationName="GUMisslMn_KATA"
								  AnimationMode="ONCE" />
                </AnimationState>
                <AnimationState
							ParseCondStateType="PARSE_TRANSITION"
							StateName="TRANS_CoverToCoverAttention">
                    <Animation
								  AnimationName="GUMisslMn_KBTA"
								  AnimationMode="ONCE" />
                </AnimationState>
			</ScriptedModelDraw>
		</Draws>
