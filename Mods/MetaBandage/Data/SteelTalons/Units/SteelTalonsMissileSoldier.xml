						AnimationMode="ONCE" />
					<Animation
						AnimNickName="DTB"
						AnimationName="GUMisslMn_DIEC"
						AnimationMode="ONCE" />
				</AnimationState>
				<!-- SUPPRESSION ANIMATIONS -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SUPPRESSED MOVING"
