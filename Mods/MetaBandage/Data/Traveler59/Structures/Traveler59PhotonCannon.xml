			<StructureUnpackUpdate
				UnpackTime="1.5s" />
			<StealthDetectorUpdate
				id="ModuleTag_StealthDetect"
				DetectionRange="200" />
			<WeaponSetUpdate
				id="ModuleTag_WeaponSetUpdate">
				<WeaponSlotTurret
					ID="1"
					AllowInterleavedFiring="false"
					InterleavedStyle="INTERLEAVE_FIRST_AVAILABLE"
					WeaponChoiceCriteria="PREFER_MOST_DAMAGE">
					<Weapon
						Ordering="PRIMARY_WEAPON"
						Template="AlienPhotonCannonWeapon" />
					<Weapon
						Ordering="PRIMARY_WEAPON"
						Template="AlienPhotonCannonShardLauncherWeapon"
						ObjectStatus="WEAPON_UPGRADED_01" />						
					<TurretSettings
						TurretTurnRate="200"
						TurretPitchRate="100"
						AllowsPitch="true"
						MinimumPitch="-10d"
						PitchHeight="90%"
						MinIdleScanTime="1.0s"
						MaxIdleScanTime="5.0s"
						MinIdleScanAngle="0.0"
						MaxIdleScanAngle="90.0">
						<TurretAITargetChooserData />
					</TurretSettings>
				</WeaponSlotTurret>
			</WeaponSetUpdate>
			<StatusBitsUpgrade
				id="ModuleTag_Weapon_Upgrade_01"
				StatusToSet="WEAPON_UPGRADED_01">
				<TriggeredBy>Upgrade_AlienShardLaunchers</TriggeredBy>
			</StatusBitsUpgrade>	
			<SubObjectsUpgrade
				id="ModuleTag_ShowShardLauncher"
				ShowSubObjects="UG_SHARD UG_SHARDWEAPON">
				<TriggeredBy>Upgrade_AlienShardLaunchers</TriggeredBy>
			</SubObjectsUpgrade>			
			<RepairUpdate 
				id="ModuleTag_RepairUpdate">
