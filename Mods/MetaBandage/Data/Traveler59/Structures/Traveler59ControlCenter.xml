<?xml version="1.0" encoding="utf-8"?>
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xi="http://www.w3.org/2001/XInclude">
<!--Buzzer Swarm spawns T59 Buzzers-->
	<Tags></Tags>
	<Includes>
	</Includes>
	<!-- NerveCenter -->
	<GameObject
		id="Traveler59ControlCenter"
		inheritFrom="AlienControlCenter"
		SelectPortrait="Portrait_ALIENNerveCenter"
		ButtonImage="Portrait_ALIENNerveCenter"
		Side="Traveler59"
		EditorSorting="STRUCTURE"
		TransportSlotCount="1"
		BuildCost="1500"
		PlacementViewAngle="225d"
		BuildTime="15"
		CommandSet="Traveler59ControlCenterCommandSet"
		KindOf="STRUCTURE SELECTABLE IMMOBILE CAN_CAST_REFLECTIONS AUTO_ACQUIRABLE_BY_AI NOT_AUTOACQUIRABLE COVER POWERED FS_RADAR FS_TECHNOLOGY"
		RadarPriority="STRUCTURE"
		ProductionQueueType="MAIN_STRUCTURE"
		BuildPlacementTypeFlag="MAIN_STRUCTURE"
		EditorName="Traveler59ControlCenter"
		Description="Desc:Traveler59ControlCenter"
		TypeDescription="Type:Traveler59ControlCenter"
		EnergyProduction="-8"
		CampnessValue="2000"
		CampnessValueRadius="300"
		UnitCategory="STRUCTURE">
		<DisplayName
			xai:joinAction="Replace" xmlns:xai="uri:ea.com:eala:asset:instance">NAME:ALIENControlCenter</DisplayName>
		<GameDependency>
			<RequiredObject>Traveler59PowerProjector</RequiredObject>
			<RequiredObject>Traveler59TiberiumTower</RequiredObject>
		</GameDependency>
		<ArmorSet
			Armor="AlienControlCenterArmor"
			DamageFX="FactionStructureDamageFX" />
		<SkirmishAIInformation
			BaseBuildingLocation="BACK" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw"
				OkToChangeModelColor="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="ABControl_SKN" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STRUCTURE_UNPACKING">
					<Model
						Name="ABControlBld" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="RUBBLE">
					<Model
						Name="ABControl_D3SN" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="REALLYDAMAGED">
					<Model
						Name="ABControl_D2SN" />
					<ParticleSysBone
						BoneName="FXBone01"
						FXParticleSystemTemplate="AlienDamageFire01"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone01"
						FXParticleSystemTemplate="AlienDamageFireGlow01"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone01"
						FXParticleSystemTemplate="AlienDamageFireGlow02"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone02"
						FXParticleSystemTemplate="AlienDamageFire01"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone02"
						FXParticleSystemTemplate="AlienDamageFireGlow01"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone02"
						FXParticleSystemTemplate="AlienDamageFireGlow02"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone03"
						FXParticleSystemTemplate="AlienDamageFire01"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone03"
						FXParticleSystemTemplate="AlienDamageFireGlow01"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone03"
						FXParticleSystemTemplate="AlienDamageFireGlow02"
						FollowBone="true" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DAMAGED">
					<Model
						Name="ABControl_SKN" />
					<Texture
						Original="ABControl"
						New="ABControl_D" />
					<ParticleSysBone
						BoneName="FXBone01"
						FXParticleSystemTemplate="AlienDamageFire01"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone03"
						FXParticleSystemTemplate="AlienDamageFire01"
						FollowBone="true" />
				</ModelConditionState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STRUCTURE_UNPACKING">
					<Animation
						AnimationName="ABControlBld"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT"
					Flags="DO_NOT_PLAY_WHEN_UNPOWERED">
					<Animation
						AnimationName="ABControl_IDLA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="RUBBLE"
					StateName="STATE_Rubble"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="ABControl_D3AN"
						AnimationMode="MANUAL" />
					<Script>
						Prev = CurDrawablePrevAnimationState();
						if Prev ~= "STATE_Rubble" and Prev ~= "TRANS_IntoRubble" then
						CurDrawableSetTransitionAnimState("TRANS_IntoRubble")
						end
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="REALLYDAMAGED"
					StateName="STATE_ReallyDamaged"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="ABControl_D2AN"
						AnimationMode="MANUAL" />
					<Script>
						Prev = CurDrawablePrevAnimationState();
						if Prev ~= "STATE_ReallyDamaged" and Prev ~= "TRANS_IntoReallyDamaged" then
						CurDrawableSetTransitionAnimState("TRANS_IntoReallyDamaged")
						end
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_IntoReallyDamaged">
					<Animation
						AnimationName="ABControl_D2AN"
						AnimationMode="ONCE"
						AnimationBlendTime="0" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_IntoRubble">
					<Animation
						AnimationName="ABControl_D3AN"
						AnimationMode="ONCE"
						AnimationBlendTime="0" />
				</AnimationState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<AttributeModifierPoolUpdate id="DefaultAttributeModifierPoolUpdate"/>
			<FireWeaponWhenDead
				id="ModuleTag_CatalystDeathWeapon"
				InitiallyActive="true"
				DeathWeapon="CatalystBuildingDeathWeapon"
				ContinueToBezierDestination="false">
				<DieMuxData DeathTypes="CATALYST" />
			</FireWeaponWhenDead>
			<TemporarilyDefectUpdate
				id="ModuleTag_TemporarilyDefect"
				DefectionFrameCount="100" />
			<FXListBehavior
				id="ModuleTag_FXList">
				<DieMuxData
					DeathTypes="ALL" />
				<Event
					Index="onTransitionToDamaged"
					FX="FX_AlienBuildingDamagedL" />
				<Event
					Index="onTransitionToReallyDamaged"
					FX="FX_AlienBuildingDamagedL" />
				<Event
					Index="onTransitionToRubble"
					FX="FX_AlienBuildingRubbleL" />
			</FXListBehavior>


			<SpecialPower
				id="ModuleTag_TiberiumVibrationScan"
				SpecialPowerTemplate="SpecialPower_TiberiumVibrationScan"
				UpdateModuleStartsAttack="true"
			/>
			<RadarScanMapSpecialAbilityUpdate
				id="ModuleTag_TiberiumVibrationScanUpdate"
				SpecialPowerTemplate="SpecialPower_TiberiumVibrationScan"
				PackTime="10s">
				<IntersectionFlagsFilter
					ObjectStatus="HAS_TIBERIUM_UPGRADE" />
				<IntersectionFlagsFilter
					KindOf="TIBERIUM_BASED HARVESTER FS_MONEY_STORAGE" />
				<IntersectionFlagsFilter
					KindOf="VEHICLE"
					ObjectStatus="HAS_TIBERIUM_AMMO" />
				<IntersectionFlagsFilter
					KindOf="TIBERIUM"
					UseLocalPlayerFilter="false" />
			</RadarScanMapSpecialAbilityUpdate>
			<AISpecialPowerUpdate
				id="ModuleTag_ControlCenterTiberiumVibration"
				CommandButtonName="Command_TiberiumVibrationScan"
				SpecialPowerRadius="999999.0"
				SpecialPowerAIType="SPECIAL_POWER_TARGET_STEALTH"
			/>


			<StructureUnpackUpdate
				id="StructureUnpackUpdate"
				UnpackTime="2.0s" />

			<!-- Seed Tiberium -->
			<OCLSpecialPower
				id="ModuleTag_OCLSpecialPowerAlienSeedTiberium"
				SpecialPowerTemplate="SpecialPower_AlienSeedTiberium"
				OCL="OCL_AlienSeedTiberiumLaserFX"
				CreateLocation="CREATE_AT_LOCATION"
			/>
			<AISpecialPowerUpdate
				id="ModuleTag_AlienCastSeedTiberium"
				CommandButtonName="Command_AlienSeedTiberium"
				SpecialPowerAIType="SPECIAL_POWER_SEED_TIBERIUM" 
			/>			
			

			<SlowDeath
				id="ModuleTag_Death"
				SinkDelay="3.0s"
				SinkRate="4.0"
				DestructionDelay="8.0s">
				<Sound Type="INITIAL" List="ALI_Building_Die" />
				<DieMuxData
					DeathTypes="ALL" />
			</SlowDeath>
			<ProductionUpdate
				id="ModuleTag_ProductionUpdate"
				Type="UPGRADE" 
				GiveNoXP="true" />

			<OCLSpecialPower
				id="ModuleTag_LightningSpikeOCL"
				SpecialPowerTemplate="SpecialPower_LightningSpike"
				OCL="OCL_AlienControlCenterLightningSpike"
				CreateLocation="CREATE_AT_LOCATION"
			/>
			<AISpecialPowerUpdate
				id="ModuleTag_AlienLightningSpike"
				CommandButtonName="Command_LightningSpike"
		        ReinforceDistance="100.0" 
				SpecialPowerRadius="300.0"
        SpellMakesAStructure="AlienLightningSpike" 
				SpecialPowerAIType="SPECIAL_POWER_REINFORCEMENT"
			/>
<!-- Tiberium Hive -->			
			<OCLSpecialPower
				id="ModuleTag_TibHiveOCL"
				SpecialPowerTemplate="SpecialPower_SummonTibHive"
				OCL="OCL_AlienTibHive"
				CreateLocation="CREATE_AT_LOCATION"
			/>
			<AISpecialPowerUpdate
				id="ModuleTag_AlienTibHive"
				CommandButtonName="Command_SummonTibHive"
		        ReinforceDistance="100.0" 
				SpecialPowerRadius="300.0"
				SpellMakesAStructure="AlienTibHive" 
				SpecialPowerAIType="SPECIAL_POWER_REINFORCEMENT"
			/>

			<RepairUpdate 
				id="ModuleTag_RepairUpdate">
				<PercentOfBuildCostToRebuildInfo></PercentOfBuildCostToRebuildInfo>
				<SelfAudio></SelfAudio>
			</RepairUpdate>
			<EngineerContain 
				id="ModuleTag_EngineerContain"
				NameOfVoiceToUseForFriendlyEnter="VoiceEngineerRepair"
				NameOfVoiceToUseForHostileEnter="VoiceCaptureBuilding" 
				EvaEventForRepair="BuildingRepaired"
				EvaEventForCapture="EnemyBuildingCaptured"
				FXForRepair="FX_Building_Repaired" 
				FXForCapture="FX_Building_Captured"
				FXForCaptureAndReplace="FX_Building_Captured"
				ImmediatelyEnabled="true">
				<CanEnterFilter Rule="NONE">
					<IncludeThing>GDIEngineer</IncludeThing>
					<IncludeThing>NODSaboteur</IncludeThing>
					<IncludeThing>AlienAssimilator</IncludeThing>
					<IncludeThing>SteelTalonsCombatEngineer</IncludeThing>
					<IncludeThing>ZOCOMEngineer</IncludeThing>
					<IncludeThing>BlackHandSaboteur</IncludeThing>
					<IncludeThing>MarkedOfKaneSaboteur</IncludeThing>
					<IncludeThing>Reaper17Assimilator</IncludeThing>
					<IncludeThing>Traveler59Assimilator</IncludeThing>
				</CanEnterFilter>
			</EngineerContain>
			<CreateObjectDie
				id="ModuleTag_CreateOccupiedDie"
				CreationList="OCL_GenericTraveler59BuildingDestruction" >
				<DieMuxData
					DeathTypes="ALL"
					DeathTypesForbidden="DETONATED SUICIDED" />
				<UpgradeForbidden>Upgrade_NoSpawnUponDestruction</UpgradeForbidden>
			</CreateObjectDie>
			<CreateObjectDie
