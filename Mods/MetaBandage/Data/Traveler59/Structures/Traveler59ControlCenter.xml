				<DieMuxData DeathTypes="SUICIDED" />
			</CreateObjectDie>
			
			<OCLSpecialPower
				id="ModuleTag_SwarmOCL"
				SpecialPowerTemplate="SpecialPower_Traveler59Swarm"
				TriggerFX="FX_AlienBlinkSwarm" 
				OCL="OCL_Traveler59Buzzers"
				CreateLocation="CREATE_AT_LOCATION">
			</OCLSpecialPower>
			<AISpecialPowerUpdate
				id="ModuleTag_AlienSwarmAI"
				CommandButtonName="Command_SpecialPowerTraveler59Swarm"
				ReinforceDistance="0.0"
				SpecialPowerRadius="250.0"
				SpecialPowerAIType="SPECIAL_POWER_REINFORCEMENT"
			/>
