<?xml version="1.0" encoding="utf-8"?>
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xi="http://www.w3.org/2001/XInclude">
	<Tags></Tags>
	<Includes>
	</Includes>
	<!-- Assembler -->
	<GameObject
		id="Traveler59Gateway"
		inheritFrom="AlienGateway"
		SelectPortrait="Portrait_ALIENWarpSphere"
		ButtonImage="Portrait_ALIENWarpSphere"
		Side="Traveler59"
		EditorSorting="STRUCTURE"
		TransportSlotCount="1"
		BuildCost="2000"
		BuildTime="20"
		CommandSet="Traveler59GatewayCommandSet"
		PlacementViewAngle="315d"
		KindOf="STRUCTURE SELECTABLE IMMOBILE CAN_CAST_REFLECTIONS FS_FACTORY AUTO_RALLYPOINT AUTO_ACQUIRABLE_BY_AI NOT_AUTOACQUIRABLE COVER POWERED_POWERS_ONLY FS_WAR_FACTORY"
		RadarPriority="STRUCTURE"
		ProductionQueueType="MAIN_STRUCTURE"
		BuildPlacementTypeFlag="MAIN_STRUCTURE"
		EditorName="Traveler59Gateway"
		Description="Desc:Traveler59Gateway"
		TypeDescription="Type:Traveler59Gateway"
		EnergyProduction="-6"
		CampnessValue="2000"
		CampnessValueRadius="300"
		UnitCategory="STRUCTURE">
		<DisplayName
			xai:joinAction="Replace" xmlns:xai="uri:ea.com:eala:asset:instance">NAME:ALIENGateway</DisplayName>
		<GameDependency>
			<RequiredObject>Traveler59PowerProjector</RequiredObject>
			<RequiredObject>Traveler59TiberiumTower</RequiredObject>
		</GameDependency>
		<ArmorSet
			Armor="AlienGatewayArmor"
			DamageFX="FactionStructureDamageFX" />
		<SkirmishAIInformation
			BaseBuildingLocation="FRONT" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw"
				OkToChangeModelColor="true"
				ExtraPublicBone="FXPortal">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="ABGateway" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STRUCTURE_UNPACKING">
					<Model
						Name="ABGatewayBld_SKN" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="RUBBLE">
					<Model
						Name="ABGatewayD3SN" />
					<Texture
						Original="ABGateway"
						New="ABGateway_D" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="REALLYDAMAGED">
					<Model
						Name="ABGatewayD2SN" />
					<Texture
						Original="ABGateway"
						New="ABGateway_D" />
					<ParticleSysBone
						BoneName="FXBone01"
						FXParticleSystemTemplate="AlienDamageFire01"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone01"
						FXParticleSystemTemplate="AlienDamageFireGlow01"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone01"
						FXParticleSystemTemplate="AlienDamageFireGlow02"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone02"
						FXParticleSystemTemplate="AlienDamageFire01"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone02"
						FXParticleSystemTemplate="AlienDamageFireGlow01"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone02"
						FXParticleSystemTemplate="AlienDamageFireGlow02"
						FollowBone="true" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DAMAGED">
					<Model
						Name="ABGateway" />
					<Texture
						Original="ABGateway"
						New="ABGateway_D" />
					<ParticleSysBone
						BoneName="FXBone01"
						FXParticleSystemTemplate="AlienDamageFire01"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="FXBone02"
						FXParticleSystemTemplate="AlienDamageFire01"
						FollowBone="true" />
				</ModelConditionState>
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT">
					<!-- no anim -->
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STRUCTURE_UNPACKING">
					<Animation
						AnimationName="ABGatewayBld_AAN"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="RUBBLE"
					StateName="STATE_Rubble"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="ABGatewayD3AN"
						AnimationMode="MANUAL" />
					<Script>
						Prev = CurDrawablePrevAnimationState();
						if Prev ~= "STATE_Rubble" and Prev ~= "TRANS_IntoRubble" then
						CurDrawableSetTransitionAnimState("TRANS_IntoRubble")
						end
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="REALLYDAMAGED"
					StateName="STATE_ReallyDamaged"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="ABGatewayD2AN"
						AnimationMode="MANUAL" />
					<Script>
						Prev = CurDrawablePrevAnimationState();
						if Prev ~= "STATE_ReallyDamaged" and Prev ~= "TRANS_IntoReallyDamaged" then
						CurDrawableSetTransitionAnimState("TRANS_IntoReallyDamaged")
						end
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_IntoReallyDamaged">
					<Animation
						AnimationName="ABGatewayD2AN"
						AnimationMode="ONCE"
						AnimationBlendTime="0" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_IntoRubble">
					<Animation
						AnimationName="ABGatewayD3AN"
						AnimationMode="ONCE"
						AnimationBlendTime="0" />
				</AnimationState>
			</ScriptedModelDraw>
<!-- DRAW FX -->
			<ScriptedModelDraw
				id="ModuleTag_Construction_FX"
				OkToChangeModelColor="true"
				UseStandardModelNames="true"
				StaticModelLODMode="true">
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="NONE">
					<Model
						Name="ABGatewayFXIdle" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="ACTIVELY_CONSTRUCTING">
					<Model
						Name="ABGatewayFX" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="RUBBLE">
					<Model
						Name="ABGatewayFXDie" />
				</ModelConditionState>
				<!--Underpowered.  No particle effects.-->
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="UNDERPOWERED">
					<Model
						Name="ABGatewayFXIdle" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="AFFECTED_BY_EMP">
					<Model
						Name="ABGatewayFXIdle" />
				</ModelConditionState>
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT">
					<!-- no anim -->
					<ParticleSysBone
						BoneName="FXPortal"
						FXParticleSystemTemplate="ABGatewayIdleCenterDist"
						FollowBone="false" />
					<ParticleSysBone
						BoneName="FXPortal"
						FXParticleSystemTemplate="ABGatewayIdleCenterFlare"
						FollowBone="false" />
					<ParticleSysBone
						BoneName="FXPortal"
						FXParticleSystemTemplate="ABGatewayIdleCenterGlow"
						FollowBone="false" />
					<ParticleSysBone
						BoneName="FXPortal"
						FXParticleSystemTemplate="ABGatewayIdleCenterDarkMatter"
						FollowBone="false" />					
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="ACTIVELY_CONSTRUCTING">
					<!-- no anim -->
					<ParticleSysBone
						BoneName="FXPortal"
						FXParticleSystemTemplate="ABGatewayCenterDist"
						FollowBone="false" />
					<ParticleSysBone
						BoneName="FXPortal"
						FXParticleSystemTemplate="ABGatewayCenterFlare"
						FollowBone="false" />
					<ParticleSysBone
						BoneName="FXPortal"
						FXParticleSystemTemplate="ABGatewayCenterGlow"
						FollowBone="false" />
					<ParticleSysBone
						BoneName="FXPortal"
						FXParticleSystemTemplate="ABGatewayCenterDarkMatter"
						FollowBone="false" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="UNDERPOWERED">
					<!-- no anim -->
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="AFFECTED_BY_EMP">
					<!-- no anim -->
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="RUBBLE"
					StateName="STATE_Rubble">
					<!-- no anim -->
					<Script>
						Prev = CurDrawablePrevAnimationState();
							if Prev ~= "STATE_Rubble" and Prev ~= "TRANS_ReallyDamagedToRubble" then CurDrawableSetTransitionAnimState("TRANS_ReallyDamagedToRubble") end;
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_ReallyDamagedToRubble">
					<Animation
						AnimationName="ABGatewayFXDie"
						AnimationMode="ONCE" />
					<ParticleSysBone
						BoneName="FXPortal"
						FXParticleSystemTemplate="ABGatewayCenterDist"
						FollowBone="false" />
					<ParticleSysBone
						BoneName="FXPortal"
						FXParticleSystemTemplate="ABGatewayCenterFlare"
						FollowBone="false" />
					<ParticleSysBone
						BoneName="FXPortal"
						FXParticleSystemTemplate="ABGatewayCenterGlow"
						FollowBone="false" />
					<ParticleSysBone
						BoneName="FXPortal"
						FXParticleSystemTemplate="ABGatewayCenterDarkMatter"
						FollowBone="false" />
				</AnimationState>
			</ScriptedModelDraw>
			<!-- draw a decal when this thing is selected -->
			<!-- This is duplicated in several files. Please do a search and update those too -->
			<ScriptedModelDraw
				id="ModuleTag_Draw_Selected_Repair_Decal"
				OkToChangeModelColor="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model Name="" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SELECTED">
					<Model Name="FXInvisBone" />
					<ShadowInfo
						Type="ADDITIVE_DECAL"
						SizeX="340"
						SizeY="340"
						OverrideLODVisibility="true"
						MaxHeight="200"
						UseHouseColor="true"
						LocalPlayerOnly="true"
						Texture="Decal_Rig_CO" 
					/>
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<AttributeModifierPoolUpdate id="DefaultAttributeModifierPoolUpdate"/>
			<FireWeaponWhenDead
				id="ModuleTag_CatalystDeathWeapon"
				InitiallyActive="true"
				DeathWeapon="CatalystBuildingDeathWeapon"
				ContinueToBezierDestination="false">
				<DieMuxData DeathTypes="CATALYST" />
			</FireWeaponWhenDead>
			<TemporarilyDefectUpdate
				id="ModuleTag_TemporarilyDefect"
				DefectionFrameCount="100" />
			<FXListBehavior
				id="ModuleTag_FXList">
				<DieMuxData
					DeathTypes="ALL" />
				<Event
					Index="onTransitionToDamaged"
					FX="FX_AlienBuildingDamagedL" />
				<Event
					Index="onTransitionToReallyDamaged"
					FX="FX_AlienBuildingDamagedL" />
				<Event
					Index="onTransitionToRubble"
					FX="FX_AlienBuildingRubbleL" />
			</FXListBehavior>
			<ProductionUpdate
				id="ModuleTag_06"
				GiveNoXP="true"
				Type="VEHICLE" />
			<QueueProductionExitUpdate
				id="ModuleTag_07"
				ExitDelay="1">
				<UnitCreatePoint
					x="0.0"
					y="0.0" />
				<NaturalRallyPoint
					x="90.0"
					y="0.0" />
			</QueueProductionExitUpdate>
			<StructureUnpackUpdate
				UnpackTime="2.0s" />

			<OCLSpecialPower
				id="ModuleTag_OCL_SpecialPowerAlienRepairDrones"
				SpecialPowerTemplate="SpecialPowerAlienRepairDrones"
				OCL="OCL_SpecialPowerAlienRepairDrones"
				CreateLocation="USE_OWNER_OBJECT"
			/>
			<AISpecialPowerUpdate
				id="ModuleTag_AlienRepairDrones"
				CommandButtonName="Command_AlienRepairDrones"
				SpecialPowerRadius="999999.0"
				AllyUnitInclude="VEHICLE"
				SpecialPowerAIType="SPECIAL_POWER_HEAL_AOE"
			/>

			<SpawnBehavior
				id="ModuleTag_SpawnRepairDrones"
				SpawnNumberData="3"
				InitialBurst="3"
				SpawnReplaceDelayData="10s"
				SpawnedRequireSpawner="true"
				KillSpawnsOnCaptured="true"
				SpawnInsideBuilding="true"
				KillSpawnsOnDisabled="true">
				<Die
					DeathTypes="ALL" />
				<SpawnTemplate>AlienRepairDrone</SpawnTemplate>
			</SpawnBehavior>
			<AssignSlavesTargetObjectSpecialPower
				id="ModuleTag_SpecialPowerRepairVehicle"
				SpecialPowerTemplate="SpecialPower_TargetedRepairVehicle" />
			<SlowDeath
				id="ModuleTag_Death"
				SinkDelay="4.0s"
				SinkRate="4.0"
				DestructionDelay="8.0s">
				<Sound Type="INITIAL" List="ALI_Building_Die" />
				<DieMuxData
					DeathTypes="ALL" />
			</SlowDeath>

			<RepairUpdate 
				id="ModuleTag_RepairUpdate">
				<PercentOfBuildCostToRebuildInfo></PercentOfBuildCostToRebuildInfo>
				<SelfAudio></SelfAudio>
			</RepairUpdate>
			<EngineerContain 
				id="ModuleTag_EngineerContain"
				NameOfVoiceToUseForFriendlyEnter="VoiceEngineerRepair"
				NameOfVoiceToUseForHostileEnter="VoiceCaptureBuilding" 
				EvaEventForRepair="BuildingRepaired"
				EvaEventForCapture="EnemyBuildingCaptured"
				FXForRepair="FX_Building_Repaired" 
				FXForCapture="FX_Building_Captured"
				FXForCaptureAndReplace="FX_Building_Captured"
				ImmediatelyEnabled="true">
				<CanEnterFilter Rule="NONE">
					<IncludeThing>GDIEngineer</IncludeThing>
					<IncludeThing>NODSaboteur</IncludeThing>
					<IncludeThing>AlienAssimilator</IncludeThing>
					<IncludeThing>SteelTalonsCombatEngineer</IncludeThing>
					<IncludeThing>ZOCOMEngineer</IncludeThing>
					<IncludeThing>BlackHandSaboteur</IncludeThing>
					<IncludeThing>MarkedOfKaneSaboteur</IncludeThing>
					<IncludeThing>Reaper17Assimilator</IncludeThing>
					<IncludeThing>Traveler59Assimilator</IncludeThing>
			</CanEnterFilter>
			</EngineerContain>
			<CreateObjectDie
				id="ModuleTag_CreateOccupiedDie"
				CreationList="OCL_GenericTraveler59BuildingDestruction" >
				<DieMuxData
					DeathTypes="ALL"
					DeathTypesForbidden="DETONATED SUICIDED" />
				<UpgradeForbidden>Upgrade_NoSpawnUponDestruction</UpgradeForbidden>
			</CreateObjectDie>
			<CreateObjectDie
				id="ModuleTag_CreateOccupiedSuicide"
				CreationList="OCL_GenericTraveler59BuildingSuicide" >
				<DieMuxData DeathTypes="SUICIDED" />
			</CreateObjectDie>
			<StancesBehavior
				id="ModuleTag_Stance"
				StanceTemplate="Generic" />
		</Behaviors>
		<AI>
			<AIUpdate
				id="ModuleTag_AI"
				AutoAcquireEnemiesWhenIdle="NO"></AIUpdate>
